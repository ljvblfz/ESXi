             <tagName>IsEsxcliOp</tagName>
             <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>3</limit>
     </policy>
     <policy id="LimitEsxcliAccountPermissionOp">
       <description>Limit the number of esxcli account/permission ops</description>
       <enabled>True</enabled>
       <matchCriterionList>
          <criterion id="1">
             <tagName>IsEsxcliAccountPermissionOp</tagName>
             <value>1</value>
          </criterion>
