VmDiskFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmDiskFailedEvent">\
        <description>\
           由於事件訊息中所述原因，無法為虛擬機器建立虛擬磁碟\
        </description>\
        <cause>\
           <description>\
                沒有為虛擬機器建立虛擬磁碟。在該作業無法存取磁碟、磁碟空間不足、您不具有執行該作業的權限等條件下，可能會發生此情況。\
           </description>\
           <action>\
                在事件訊息中查看原因以找到失敗原因。確認磁碟可供存取並具有足夠的空間，且權限設定允許執行該作業。\
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmFailedMigrateEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedMigrateEvent">\
        <description>\
           由於事件訊息中所述原因，無法移轉虛擬機器\
        </description>\
        <cause> \
            <description> \
                未移轉虛擬機器。在未設定 vMotion IP、來源主機和目的地主機無法存取等條件下，可能會發生此情況。\
            </description> \
            <action> \
                在事件訊息中查看原因以找到失敗原因。確認來源主機和目的地主機已設定 vMotion IP、這些主機可供存取等事項。\
            </action> \
        </cause> \
     </EventLongDescription>\
     "

VmFailedRelayoutOnVmfs2DatastoreEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedRelayoutOnVmfs2DatastoreEvent">\
        <description>\
           無法移轉 VMFS2 資料存放區上的虛擬機器\
        </description>\
        <cause>\
           <description>\
                嘗試移轉虛擬機器失敗，因為虛擬機器在 VMFS2 資料存放區上仍有磁碟。對於 ESX 3.0 及更新版本的主機，VMFS2 資料存放區為唯讀。\
           </description>\
           <action> \
                將資料存放區從 VMFS2 升級至 VMFS3\
           </action> \
        </cause>\
     </EventLongDescription>\
     "

VmFailedRelayoutEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedRelayoutEvent">\
        <description>\
           無法配置虛擬機器 \
        </description>\
        <cause>\
           <description>\
                由於事件訊息中所述原因，嘗試在磁碟上配置虛擬機器失敗。此情況可能是由多種原因中的任一種造成，例如，無法存取磁碟。\
           </description>\
        <action>\
	    在事件訊息中查看原因以找到失敗原因，並更正問題。\
         </action>\
	</cause>\
     </EventLongDescription>\
     "

VmAcquiredMksTicketEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmAcquiredMksTicketEvent">\
        <description>\
           已成功取得虛擬機器的 MKS 票證\
        </description>\
        <cause>\
           <description>\
                已成功取得用於連線到虛擬機器遠端主控台的 MKS 票證。\
           </description>\
        </cause>\
     </EventLongDescription>\
     "

VmWwnAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnAssignedEvent">\
      <description>\
         已向虛擬機器指派 WWN (全球名稱)\
      </description>\
      <cause>\
         <description>由於虛擬機器是使用 RDM (原始裝置對應) 磁碟建立的或重新設定為存取 RDM 磁碟，因此向虛擬機器指派了 WWN\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmWwnChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnChangedEvent">\
      <description>\
         指派給虛擬機器的 WWN (全球名稱) 已變更 \
      </description>\
      <cause>\
         <description>已向虛擬機器指派新的 WWN，這可能是由於向另一台虛擬機器指派了相同的 WWN 而導致衝突\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmWwnConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnConflictEvent">\
      <description>\
         指派給虛擬機器的 WWN (全球名稱) 存在衝突 \
      </description>\
      <cause>\
         <description>指派給此虛擬機器的 WWN 與其他虛擬機器的 WWN 相同。\
       </description>\
      <action>\
	    查看事件詳細資料以瞭解有關衝突的詳細資訊，並更正問題。\
         </action>\</cause>\
   </EventLongDescription>\
   "

VmReloadFromPathFailedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmReloadFromPathFailedEvent">\
      <description>\
         從新的資料存放區路徑重新載入虛擬機器失敗 \
      </description>\
      <cause>\
         <description>目的地資料存放區路徑無法存取或無效\
         </description>\
         <action>使用有效的目的地資料存放區路徑 \
         </action> \
      </cause>\
      <cause>\
         <description>虛擬機器處於無效狀態 \
         </description>\
         <action>檢查虛擬機器的電源狀態。如果虛擬機器已開啟電源，請關閉其電源 \
         </action>\
      </cause>\
      <cause>\
         <description>虛擬機器已啟用 Fault Tolerance\
         </description>\
         <action>停用虛擬機器的 Fault Tolerance，然後重試該作業 \
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationLinuxIdentityFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationLinuxIdentityFailed">\
      <description>\
         自訂指令碼不支援客體作業系統 Linux 散發。如需支援的 Linux 散發清單，請參閱《VMware vSphere 相容性矩陣圖》。\
      </description>\
      <cause>\
         <description>\
            不支援自訂目標客體作業系統 Linux 散發。\
         </description>\
         <action>\
            諮詢 VMware 以瞭解何時會支援特定 Linux 散發。如果較新版本已支援 Linux 散發，請考慮進行升級。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationNetworkSetupFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationNetworkSetupFailed">\
      <description>\
         自訂指令碼無法在對應的 Linux 組態檔或 Windows 登錄中設定參數\
      </description>\
      <cause>\
         <description>\
            自訂規格包含無效的主機名稱或網域名稱\
         </description>\
         <action>\
            檢閱此事件的客體作業系統記錄檔，以瞭解詳細資料\
         </action>\
         <action>\
            為目標客體作業系統提供有效的主機名稱。該名稱必須符合 RFC 952、1035、1123、2181 中的主機名稱和網域名稱定義。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            找不到具有自訂套件中所指定 MAC 位址的 NIC \
         </description>\
         <action>\
            檢閱此事件的客體作業系統記錄檔，以瞭解詳細資料\
         </action>\
         <action>\
            確認從建立到部署自訂套件的期間未變更虛擬 NIC MAC 位址。排程自訂後，在虛擬機器首次開機時會進行部署。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            自訂程式碼需要特定組態檔的讀取/寫入權限。這些權限未授予給 Linux 上的「root」帳戶，也未授予給 Windows 客體作業系統上 VMware Tools 服務所使用的帳戶。\
         </description>\
         <action>\
            檢閱此事件的客體作業系統記錄檔，以瞭解詳細資料\
         </action>\
         <action>\
            將讀取/寫入權限授予給 Linux 的「root」帳戶，或授予給 Windows 客體作業系統上 VMware Tools 服務所使用的帳戶，以及需要由自訂程式碼修改的登錄機碼\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationSysprepFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationSysprepFailed">\
      <description>\
         與所選目標客體作業系統對應之資料夾中的 sysprep 檔案，與該客體作業系統的實際版本不相容\
      </description>\
      <cause>\
         <description>\
            與目標客體作業系統 (如 Windows XP) 對應之資料夾中的 sysprep 檔案適用於其他客體作業系統 (如 Windows 2003)\
         </description>\
         <action>\
            在執行 vCenter Server 的電腦上，將正確的 sysprep 檔案置於與目標客體作業系統對應的資料夾中\
         </action>\
      </cause>\
      <cause>\
         <description>\
            與客體作業系統對應之資料夾中的 sysprep 檔案適用於其他 Service Pack，例如，客體作業系統是 Windows XP SP2，但 sysprep 檔案卻適用於 Windows XP SP1。\
         </description>\
         <action>\
            在執行 vCenter Server 的電腦上，將正確的 sysprep 檔案置於與目標客體作業系統對應的資料夾中\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationUnknownFailure.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationUnknownFailure">\
      <description>\
         自訂元件無法在客體作業系統內設定必要參數\
      </description>\
      <cause>\
         <description>\
            在 Windows 上，自訂程式碼在使用者帳戶之下執行，但該使用者帳戶不具有自訂程式碼所使用的登錄機碼的讀取/寫入權限。自訂程式碼通常是在「本機系統」帳戶之下執行，但您可以為 VMware Tools 服務執行選取其他帳戶來加以變更。\
         </description>\
         <action>\
            檢閱此事件的客體作業系統記錄檔，以瞭解詳細資料\
         </action>\
         <action>\
            判定為 VMware Tools 服務執行選取的使用者帳戶，並確認此帳戶對自訂程式碼使用的登錄機碼具有讀取/寫入權限\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Windows 上，自訂程式碼在使用者帳戶之下執行，該使用者帳戶不具有自訂程式碼所使用的檔案和資料夾的讀取/寫入權限。自訂程式碼通常是在「本機系統」帳戶之下執行，但您可以為 VMware Tools 服務執行選取其他帳戶來加以變更。\
         </description>\
         <action>\
            檢閱此事件的客體作業系統記錄檔，以瞭解詳細資料\
         </action>\
         <action>\
            判定為 VMware Tools 服務執行選取的使用者帳戶，並確認此帳戶對自訂程式碼使用的檔案和資料夾具有讀取/寫入權限\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Linux 上，向自訂指令碼傳遞了無效或不支援的時區，導致時區組態失敗\
         </description>\
         <action>\
            檢閱此事件的客體作業系統記錄檔，以瞭解詳細資料\
         </action>\
         <action>\
            確認在自訂規格中傳遞的是支援的時區。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Linux 上，客體作業系統「root」帳戶對自訂指令碼需要修改的組態檔 (「/etc/hosts」) 不具有讀取/寫入權限\
         </description>\
         <action>\
            將這些組態檔的讀取/寫入權限授予給客體作業系統「root」帳戶\
         </action>\
      </cause>\
      <cause>\
         <description>\
            若要在 Linux 上啟用客體自訂，如果已使用開啟虛擬機器工具，您還必須安裝部署套件外掛程式。\
         </description>\
         <action>\
            依照 kb.vmware.com/kb/2075048 安裝開啟虛擬機器工具部署套件外掛程式。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            不支援自訂目標客體作業系統\
         </description>\
         <action>\
            請諮詢 VMware 以瞭解何時會支援特定 Linux 散發。如果較新版本已支援 Linux 散發，請考慮進行升級。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "
#
# FT Related Events
#

VmFailedStartingSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedStartingSecondaryEvent">\
       <description>\
          由於發生錯誤，vCenter Server 無法啟動次要虛擬機器\
       </description>\
       <cause>\
            <description>\
              遠端主機與次要虛擬機器不相容。例如，當主機無權存取虛擬機器的網路或資料存放區時，便可能會發生此情況。\
            </description>\
            <action>確認叢集中的主機與 FT 相容</action>\
        </cause>\
        <cause>\
            <description>登入遠端主機失敗。如果該主機是新增至詳細目錄或剛剛重新開機，則 SSL 憑證指紋可能需要一點時間才能散佈到主機。\
            </description>\
            <action>如果問題仍然存在，請中斷主機連線後再重新連線。</action>\
        </cause>\
        <cause>\
            <description>在遠端主機上登錄次要虛擬機器失敗</description>\
            <action>判定遠端主機是否有權存取 FT 虛擬機器所在的資料存放區</action>\
        </cause>\
        <cause>\
            <description>啟動次要虛擬機器時發生錯誤</description>\
            <action>判定移轉錯誤的原因。vCenter Server 將在可能的情況下嘗試重新啟動次要虛擬機器。</action>\
        </cause>\
    </EventLongDescription>\
    "

VmFailedUpdatingSecondaryConfig.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedUpdatingSecondaryConfig">\
       <description>\
          在容錯移轉後，新的主要虛擬機器無法更新次要虛擬機器的組態\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "

VmFaultToleranceStateChangedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceStateChangedEvent">\
       <description>\
          虛擬機器的 Fault Tolerance 狀態已變更\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "

VmFaultToleranceTurnedOffEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceTurnedOffEvent">\
       <description>\
          所有次要虛擬機器均已移除，且此虛擬機器的 Fault Tolerance 保護已關閉。\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "


VmFaultToleranceVmTerminatedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceVmTerminatedEvent">\
       <description>\
          主要虛擬機器或次要虛擬機器變為非作用中狀態\
       </description>\
       <cause>\
            <description> 次要虛擬機器變為非作用中狀態，因為其作業不再與主要虛擬機器的作業同步</description>\
            <action>vSphere HA 將嘗試重新啟動次要虛擬機器</action>\
        </cause>\
       <cause>\
            <description> 次要虛擬機器變為非作用中狀態，因為硬體或網路故障導致主要虛擬機器與次要虛擬機器中斷連線</description>\
            <action>vSphere HA 將嘗試重新啟動次要虛擬機器</action>\
        </cause>\
       <cause>\
            <description> 容錯虛擬機變為非作用中，因為實體主機上的部分硬體發生故障</description>\
            <action>vSphere HA 將嘗試重新啟動次要虛擬機器</action>\
        </cause>\
       <cause>\
            <description> 使用者已停止容錯虛擬機器</description>\
            <action>剩餘的容錯虛擬機器會接任為主要虛擬機器。vSphere HA 將嘗試重新啟動次要虛擬機器。</action>\
        </cause>\
    </EventLongDescription>\
    "


VmMaxFTRestartCountReached.longDescription = "\
    <EventLongDescription id="vim.event.VmMaxFTRestartCountReached">\
       <description>\
          系統在嘗試重新啟動次要虛擬機器時已達到重新啟動次數上限\
       </description>\
       <cause>\
            <description>系統在嘗試重新建立 Fault Tolerance 時，已超過允許的次要虛擬機器重新啟動嘗試次數</description>\
            <action>檢查重新啟動失敗的原因並進行修復。然後，停用 Fault Tolerance 保護後再重新啟用。</action>\
        </cause>\
    </EventLongDescription>\
    "

VmNoCompatibleHostForSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmNoCompatibleHostForSecondaryEvent">\
       <description>\
          嘗試放置次要虛擬機器時找不到相容主機\
       </description>\
       <cause>\
            <description>沒有可用於放置次要虛擬機器的相容主機</description>\
            <action>解決不相容問題，然後重試該作業</action>\
        </cause>\
    </EventLongDescription>\
    "

VmSecondaryDisabledBySystemEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmSecondaryDisabledBySystemEvent">\
       <description>\
          由於無法開啟次要虛擬機器的電源，vCenter Server 已停用該次要虛擬機器\
       </description>\
       <cause>\
            <description>vCenter Server 無法開啟次要虛擬機器的電源 </description>\
            <action>在事件訊息中查看原因以瞭解詳細資料、修復失敗問題，然後重新啟用 Fault Tolerance 保護以開啟次要虛擬機器的電源。</action>\
        </cause>\
    </EventLongDescription>\
    "


VmTimedoutStartingSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmTimedoutStartingSecondaryEvent">\
       <description> 嘗試啟動次要虛擬機器逾時。\
       </description>\
       <cause>\
            <description>使用者嘗試開啟或啟用 Fault Tolerance，由此觸發次要虛擬機器的啟動。啟動作業逾時，因此，vCenter Server 會停用 Fault Tolerance。 </description>\
            <action>修復任何問題並重新啟用 Fault Tolerance 保護</action>\
        </cause>\
       <cause>\
            <description>失敗導致次要虛擬機器啟動，但啟動嘗試逾時</description>\
            <action> vSphere HA 將嘗試開啟次要虛擬機器的電源</action>\
        </cause>\
    </EventLongDescription>\
    "

CanceledHostOperationEvent.longDescription = "\
	<EventLongDescription id="vim.event.CanceledHostOperationEvent">\
		<description>\
			已取消對主機執行的作業\
		</description>\
		<cause>\
			<description>\
		一系列事件中的前一個事件將提供有關此次取消作業之原因的詳細資訊\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

ChangeOwnerOfFileFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.ChangeOwnerOfFileFailedEvent">\
		<description>\
			變更檔案或資料夾的擁有者失敗\
		</description>\
	</EventLongDescription>\
    "

HostAddFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostAddFailedEvent">\
		<description>\
			新增主機失敗\
		</description>\
	</EventLongDescription>\
    "

HostAdminDisableEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostAdminDisableEvent">\
		<description>\
			主機權限已變更，因此，只有用於 vCenter Server 作業的帳戶才具有系統管理員權限\
		</description>\
		<cause>\
			<description>\
				由於主機已置於鎖定模式而使 vCenter Server 移除對主機的所有其他系統管理員存取權時，便會發生此情況。主機只能由 vCenter Server 管理，並且只有 vCenter Server 才能重新啟用其他帳戶的系統管理員存取權。\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

HostAdminEnableEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostAdminEnableEvent">\
		<description>\
			對於權限遭鎖定模式停用的主機使用者帳戶，vCenter Server 已還原其系統管理員權限\
		</description>\
		<cause>\
			<description>\
				當 vCenter Server 還原主機使用者帳戶 (在主機置於鎖定模式時失去系統管理員權限) 的系統管理員存取權時，便會發生此情況\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedAccountFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedAccountFailedEvent">\
		<description>\
			由於設定管理帳戶失敗，無法連線到主機\
		</description>\
		<cause>\
			<description>\
			無法設定 vCenter Server 用於管理主機的帳戶\
			</description>\
		</cause>\
	</EventLongDescription>\
