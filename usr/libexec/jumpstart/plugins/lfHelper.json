/*
 * lfHelper.json --
 *
 *	Jumpstart plug-in to load LFHelper kernel module.
 */


{
   "operations" :
   {
      "start" :
      [
         "system module load --module lfHelper"
      ]
   }
   ,
   "metadata" :
   {
      "version"  : 1,
      "requires" : ["vmkeventd"],
      "provides" : ["lfHelper"]
   }
}
