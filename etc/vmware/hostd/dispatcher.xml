       <enabled>True</enabled>
       <matchCriterionList>
          <criterion id="1">
             <tagName>IsHostProfilesEditThrottledOp</tagName>
            <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>1</limit>
     </policy>
     <policy id="LimitCBRCThrottledOp">
