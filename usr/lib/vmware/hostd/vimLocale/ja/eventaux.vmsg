  "

DatastoreDiscoveredEvent.longDescription = "\
  <EventLongDescription id="vim.event.DatastoreDiscoveredEvent">\
     <description>\
        ホスト上でデータストアが検出されました。\
     </description>\
     <cause>\
        <description>\
           このデータストアに対するアクセス権限を持つホストがデータセンターに追加されました。\
        </description>\
     </cause>\
     <cause>\
        <description>\
           このデータストアの背後にあるストレージは、データセンター内のホストに対してマスク解除されました。\
        </description>\
     </cause>\
     <cause>\
        <description>\
           ユーザー アクションまたはシステム アクションにより、このデータストアがホスト上で作成されました。\
        </description>\
     </cause>\
     <cause>\
        <description>\
           ユーザー アクションまたはシステム アクションにより、このデータストアがホスト上で作成されました。この操作が行われる前、データストアはデータセンター内の少なくとも 1 台の他のホスト上で表示されていました。\
        </description>\
     </cause>\
   </EventLongDescription>\
   "

DatastoreRenamedOnHostEvent.longDescription = "\
   <EventLongDescription id="vim.event.DatastoreRenamedOnHostEvent">\
      <description>\
         vCenter Server により管理されているホスト上で、データストアの名前が変更されました。\
      </description>\
      <cause>\
         <description>\
            vCenter Server はホスト上でデータストアを検出し、そのデータストアが別の名前で vCenter Server インベントリにすでに存在しているため、そのデータストアの名前を変更しました。 または、同じデータセンター内の別のデータストアの名前と競合していたために、データストアの名前を変更した可能性があります。\
         </description>\
      </cause>\
   </EventLongDescription>\
  "

VMFSDatastoreExtendedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VMFSDatastoreExtendedEvent">\
      <description>\
         既存の VMFS ボリュームが拡張され、キャパシティが増加しました。\
      </description>\
      <cause>\
         <description>\
            ユーザー アクションまたはシステム アクションにより、データストアが LUN 上のパーティションを使用して拡張され、キャパシティが増加しました。\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VMFSDatastoreExpandedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VMFSDatastoreExpandedEvent">\
      <description>\
         VMFS ボリュームの既存のエクステントが増やされ、キャパシティが増加しました。\
      </description>\
      <cause>\
         <description>\
            ユーザー アクションまたはシステム アクションにより、既存の VMFS データストアのエクステントが増やされました。 エクステントは隣接して空き容量がある場合のみ拡張可能です。 そのため、このアクションによって LUN 上の隣接する容量が利用されました。\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmConfigMissingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmConfigMissingEvent">\
      <description>\
         仮想マシンの 1 つまたは複数の構成ファイルが見つかりません。\
