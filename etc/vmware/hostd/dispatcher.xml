<dispatchPolicyConfig>
   <limitPolicies>
     <policy id="LimitNetworkConfig">
       <description>Limit the number of NetworkSystem methods to only one at a time</description>
       <enabled>True</enabled>
       <matchCriterionList>
          <criterion id="1">
            <tagName>IsNetworkConfig</tagName>
            <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>2</limit>
     </policy>
     <policy id="LimitDvsConfig">
       <description>Limit the number of HostDvsManager methods to only one at a time</description>
       <enabled>True</enabled>
       <matchCriterionList>
          <criterion id="1">
            <tagName>IsDvsConfig</tagName>
            <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>2</limit>
     </policy>
     <policy id="LimitDatastoreBrowserSearch">
       <description>Limit the number of datastore browser searches</description>
       <enabled>True</enabled>
       <matchCriterionList>
          <criterion id="1">
            <tagName>IsDatastoreBrowserSearch</tagName>
            <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>4</limit>
     </policy>
     <policy id="LimitFoundryVMOp">
       <description>Limit the number of VM ops involving foundry</description>
       <enabled>True</enabled>
       <matchCriterionList>
          <criterion id="1">
            <tagName>IsVMFoundryOp</tagName>
            <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>60</limit>
     </policy>
     <policy id="LimitVMThrottledOp">
