HostInventoryFullEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostInventoryFullEvent">\
       <description>\
          La clé de licence vCenter Server Foundation autorise actuellement l'ajout de trois hôtes dans  l'inventaire uniquement. L'ajout d'hôtes supplémentaires engendre des erreurs  et l'enregistrement de cet événement. \
       </description>\
       <cause>\
            <description>Tentative d'ajouter un nombre d'hôtes plus important que celui autorisé par la clé de licence  assignée à vCenter Server</description>\
            <action>Assignez à vCenter Server une clé de licence qui autorise un plus grand nombre d'hôtes ou  sans limite d'hôte</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseNonComplianceEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseNonComplianceEvent">\
       <description>\
         vCenter Server n'impose pas strictement l'utilisation de la licence. Au lieu de cela, il vérifie l'utilisation abusive de la licence  périodiquement. Si vCenter Server détecte une utilisation abusive, il enregistre cet événement et  déclenche une alarme.\
       </description>\
       <cause>\
            <description>Surutilisation de licences</description>\
            <action>Vérifiez les rapports de licence via vSphere Client et réduisez le nombre  d'entités utilisant la clé de licence, ou ajoutez une nouvelle clé de licence disposant d'une plus grande  capacité</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseServerUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseServerUnavailableEvent">\
       <description>\
          vCenter Server assure le suivi de l'état du serveur de licences et enregistre cet événement si  le serveur de licences ne répond plus. \
       </description>\
       <cause>\
            <description>Le serveur de licences ne répond pas et n'est pas disponible pour vCenter Server</description>\
            <action>Vérifiez que le serveur de licences fonctionne. Si tel est le cas, vérifiez la connectivité entre  vCenter Server et le serveur de licences.</action>\
       </cause>\
    </EventLongDescription>\
    "

NoLicenseEvent.longDescription = "\
    <EventLongDescription id="vim.event.NoLicenseEvent">\
       <description>\
         vCenter Server enregistre cet événement s'il ne parvient pas à acquérir une fonction du serveur de licences pour  une raison inconnue. \
       </description>\
       <cause>\
            <description>L'acquisition d'une licence de fonction échoue pour une raison inconnue</description>\
            <action>Vérifiez que le serveur de licences a la licence pour la fonction</action>\
       </cause>\
    </EventLongDescription>\
    "


InvalidEditionEvent.longDescription = "\
    <EventLongDescription id="vim.event.InvalidEditionEvent">\
       <description>\
          vCenter Server a tenté d'acquérir une fonction non définie auprès du serveur de licences \
       </description>\
       <cause>\
            <description>Toute opération nécessitant une licence de fonction telle que vMotion, DRS, vSphere HA pourrait entraîner  cet événement si cette fonction n'est pas définie sur le serveur de licences</description>\
            <action>Vérifiez que la fonction en question est présente sur le serveur de licences</action>\
       </cause>\
    </EventLongDescription>\
    "

HostLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostLicenseExpiredEvent">\
       <description>\
         vCenter Server assure le suivi des délais d'expiration des licences d'hôte sur le serveur de licences  et utilise cet événement pour vous informer au sujet des licences d'hôte sur le point d'expirer\
       </description>\
       <cause>\
            <description>Les licences d'hôte sur le serveur de licences sont sur le point d'expirer</description>\
            <action>Mettez à jour le serveur de licences pour obtenir une nouvelle version de la licence d'hôte</action>\
       </cause>\
    </EventLongDescription>\
    "

VMotionLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.VMotionLicenseExpiredEvent">\
       <description>\
         vCenter Server assure le suivi des délais d'expiration des licences vMotion sur le serveur de licences  et utilise cet événement pour vous informer au sujet des licences vMotion sur le point d'expirer\
       </description>\
       <cause>\
            <description>Les licences vMotion sur le serveur de licences sont sur le point d'expirer</description>\
            <action>Mettez à jour le serveur de licences pour obtenir une version plus récente de la licence vMotion</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseRestrictedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseRestrictedEvent">\
       <description>\
         vCenter Server enregistre cet événement s'il ne peut pas extraire une licence  du serveur de licences en raison des restrictions contenues dans le fichier de licences\
       </description>\
       <cause>\
            <description>Le fichier de licence dans le serveur de licences comporte des restrictions qui empêchent l'extraction</description>\
            <action>Vérifiez le fichier de licence et supprimez autant de restrictions que possible</action>\
       </cause>\
    </EventLongDescription>\
    "

VmFailedToPowerOffEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOffEvent">\
      <description>Échec de mise hors tension de la machine virtuelle</description>\
      <cause>\
         <description>\
            La machine virtuelle exécute peut-être plusieurs opérations en même temps\
         </description>\
         <action>Mettez fin aux opérations simultanées et retentez l'opération de mise hors tension</action>\
      </cause>\
      <cause>\
         <description>La machine virtuelle est dans un état incorrect. Les machines virtuelles peuvent entrer dans  un état incorrect pour de nombreuses raisons, par exemple l'inaccessibilité de la banque de données. </description>\
         <action>\
           Identifiez la raison pour laquelle la machine virtuelle est entrée dans un état incorrect, corrigez le problème, et retentez l'opération.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToPowerOnEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOnEvent">\
      <description>\
         Échec de mise sous tension de la machine virtuelle\
      </description>\
      <cause>\
         <description>\
            Les tentatives de démarrage de la machine virtuelle peuvent échouer parce que l'état de cette dernière est déjà  en marche, d'autres systèmes fonctionnent simultanément sur la machine virtuelle, etc. </description>\
         <action>\
	  Vérifiez la raison dans le message d'événement pour trouver la cause de l'échec de mise sous tension et corriger le problème.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToRebootGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToRebootGuestEvent">\
      <description>\
         Échec de redémarrage du système d'exploitation client sur la machine virtuelle.\
      </description>\
      <cause>\
         <description>\
            L'initialisation du système d'exploitation client peut échouer parce que l'état de la machine virtuelle n'est pas  en marche, d'autres systèmes fonctionnent simultanément sur la machine virtuelle, etc.\
         </description>\
	 <action>\
	  Vérifiez la raison dans le message d'événement pour trouver la cause de l'échec de redémarrage et corriger le problème.\
         </action>\
       </cause>\
   </EventLongDescription>\
   "

VmFailedToResetEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToResetEvent">\
      <description>Échec de réinitialisation de la machine virtuelle</description>\
      <cause>\
         <description>\
		Il se peut que la machine virtuelle attende une réponse à une question ou une invite</description>\
         <action>\
           Allez à l'onglet de synthèse de la machine virtuelle dans vSphere Client et répondez à la question  ou à l'invite\
         </action>\
      </cause>\
      <cause>\
         <description>Il se peut que vous n'ayez pas suffisamment de licences disponibles pour exécuter cette opération.</description>\
         <action>\
            Obtenez les licences requises et retentez l'opération de réinitialisation\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Il se peut que des opérations simultanées soit en cours d'exécution sur la machine virtuelle\
         </description>\
         <action>Mettez fin aux opérations simultanées et retentez l'opération de réinitialisation</action>\
      </cause>\
      <cause>\
         <description>\
            L'hôte sur lequel la machine virtuelle est en cours d'exécution entre en mode maintenance\
         </description>\
         <action>\
            Attendez que l'hôte quitte le mode maintenance et retentez l'exécution\
         </action>\
      </cause>\
      <cause>\
         <description>La machine virtuelle est dans un état incorrect. Les machines virtuelles peuvent entrer dans  un état incorrect pour de nombreuses raisons, par exemple l'inaccessibilité de la banque de données.</description>\
         <action>\
           Identifiez la raison pour laquelle la machine virtuelle est entrée dans un état incorrect, corrigez le problème, et retentez l'opération.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmUpgradingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUpgradingEvent">\
      <description>Le matériel virtuel sur cette machine virtuelle est mis à jour</description>\
      <cause>\
         <description>Une action de l'utilisateur a déclenché une mise à niveau du matériel de la machine virtuelle</description>\
      </cause>\
      <cause>\
         <description>Une tâche planifiée a lancé une mise à niveau du matériel de la machine virtuelle</description>\
      </cause>\
   </EventLongDescription>\
   "

VmUuidAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUuidAssignedEvent">\
      <description>Un nouveau UUID de BIOS a été assigné à la machine virtuelle</description>\
      <cause>\
         <description>L'utilisateur qui a créé la machine virtuelle n'a pas spécifié un UUID de BIOS au moment de la création.  vCenter Server a généré un nouvel UUID et l'a affecté à la machine virtuelle.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidAssignedEvent">\
      <description>Un nouvel UUID d'instance spécifique de vCenter Server a été assigné à la machine virtuelle\
      </description>\
      <cause>\
         <description> \
             L'utilisateur qui a créé la machine virtuelle n'a pas spécifié un UUID d'instance spécifique à vCenter Server  au moment de la création. vCenter Server a généré un nouvel UUID et l'a affecté à la  machine virtuelle.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmUuidChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUuidChangedEvent">\
      <description>L'UUID de BIOS de la machine virtuelle a changé</description>\
      <cause>\
         <description>\
            Un utilisateur a changé l'UUID de BIOS de la machine virtuelle directement sur l'hôte\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidChangedEvent">\
      <description> \
         L'UUID d'instance spécifique de vCenter Server de la machine virtuelle a changé\
      </description> \
      <cause>\
         <description>\
           Une action de l'utilisateur a entraîné une modification de l'UUID d'instance spécifique de vCenter Server de  la machine virtuelle\
         </description>\
      </cause>\
      <cause>\
         <description>\
           vCenter Server a changé l'UUID d'instance de la machine virtuelle parce qu'il a détecté  un conflit\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidChangedEvent">\
      <description> \
         L'UUID d'instance spécifique de vCenter Server de la machine virtuelle était en conflit avec celui  d'une autre machine virtuelle. \
      </description>\
      <cause>\
         <description>\
            Des conflits d'UUID d'instance de machine virtuelle peuvent se produire si vous copiez des fichiers de machine virtuelle  manuellement sans utiliser vCenter Server. \
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmRelocateFailedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmRelocateFailedEvent">\
      <description>\
         Échec du déplacement d'une machine virtuelle vers un hôte ou une banque de données différent(e)\
      </description>\
      <cause>\
         <description>\
           Le déplacement de la machine virtuelle peut échouer pour un certain nombre de raisons, y compris  des pannes de réseau, l'insuffisance d'espace disque, etc\
         </description>\
         <action>\
            Examinez la tâche associée à cet événement, évaluez la raison de l'échec, et agissez en conséquence\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmUnsupportedStartingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUnsupportedStartingEvent">\
      <description>\
         Tentative de mise sous tension d'une machine virtuelle dotée d'un système d'exploitation client non pris en charge\
      </description>\
      <cause>\
         <description>\
           Une action de l'utilisateur a lancé une opération de mise sous tension d'une machine virtuelle, mais la machine virtuelle possède  un système d'exploitation client non pris en charge.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

# VmRegisteredEvent: Basic event has enough details
# VmRenamedEvent: Basic event has enough details
