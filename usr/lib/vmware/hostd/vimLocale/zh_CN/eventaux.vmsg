HostInventoryFullEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostInventoryFullEvent">\
       <description>\
          vCenter Server Foundation 许可证密钥当前只允许向清单中添加三个主机。添加多余的主机会导致出现错误，且系统也会记录此事件。 \
       </description>\
       <cause>\
            <description>尝试添加的主机数量超过分配给 vCenter Server 的许可证密钥所允许的数量</description>\
            <action>为 vCenter Server 分配允许更多主机或无任何主机限制的许可证密钥</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseNonComplianceEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseNonComplianceEvent">\
       <description>\
          vCenter Server 不会严格控制许可证的使用情况。但是，也会定期检查是否存在过度使用许可证的情况。如果 vCenter Server 检测到此类情况，则会记录此事件并触发一条警报。\
       </description>\
       <cause>\
            <description>过度使用许可证</description>\
            <action>通过 vSphere Client 检查许可证报告，并减少使用许可证密钥的实体数，或者添加一个容量更大的新许可证密钥</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseServerUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseServerUnavailableEvent">\
       <description>\
          如果许可证服务器已停止响应，则 vCenter Server 会跟踪许可证服务器的状态并记录此事件。 \
       </description>\
       <cause>\
            <description>许可证服务器未响应，无法供 vCenter Server 使用</description>\
            <action>验证许可证服务器是否正在运行。如果正在运行，请检查 vCenter Server 与许可证服务器之间的连接。</action>\
       </cause>\
    </EventLongDescription>\
    "

NoLicenseEvent.longDescription = "\
    <EventLongDescription id="vim.event.NoLicenseEvent">\
       <description>\
          如果由于未知原因而导致 vCenter Server 无法从许可证服务器获取功能，则 vCenter Server 会记录此事件。 \
       </description>\
       <cause>\
            <description>由于未知原因，获取功能许可证失败</description>\
            <action>验证许可证服务器是否具有相应功能的许可证</action>\
       </cause>\
    </EventLongDescription>\
    "


InvalidEditionEvent.longDescription = "\
    <EventLongDescription id="vim.event.InvalidEditionEvent">\
       <description>\
          vCenter Server 尝试从许可证服务器获取未定义的功能 \
       </description>\
       <cause>\
            <description>如果未在许可证服务器上定义某一功能，则需要具备相应功能许可证 (如 vMotion、DRS、vSphere HA) 的任何操作都可能导致出现此事件</description>\
            <action>验证许可证服务器上是否存在所涉及到的功能</action>\
       </cause>\
    </EventLongDescription>\
    "

HostLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostLicenseExpiredEvent">\
       <description>\
          vCenter Server 会跟踪许可证服务器上主机许可证的到期时间，并使用此事件来通知您即将到期的任何主机许可证\
       </description>\
       <cause>\
            <description>许可证服务器上的主机许可证即将到期</description>\
            <action>更新许可证服务器以获取新版本的主机许可证</action>\
       </cause>\
    </EventLongDescription>\
    "

VMotionLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.VMotionLicenseExpiredEvent">\
       <description>\
          vCenter Server 会跟踪许可证服务器上 vMotion 许可证的到期时间，并使用此事件来通知您即将到期的任何 vMotion 许可证\
       </description>\
       <cause>\
            <description>许可证服务器上的 vMotion 许可证即将到期</description>\
            <action>更新许可证服务器以获取更新版本的 vMotion 许可证</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseRestrictedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseRestrictedEvent">\
       <description>\
          如果由于许可证文件中的限制导致 vCenter Server 无法从许可证服务器签出许可证，则 vCenter Server 会记录此事件 \
       </description>\
       <cause>\
            <description>许可证服务器中的许可证文件具有防止签出的限制</description>\
            <action>检查许可证文件，并尽可能移除任何限制</action>\
       </cause>\
    </EventLongDescription>\
    "

VmFailedToPowerOffEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOffEvent">\
      <description>无法关闭虚拟机电源</description>\
      <cause>\
         <description>\
            虚拟机可能正在执行并发操作\
         </description>\
         <action>完成并发操作，然后重试关闭电源操作</action>\
      </cause>\
      <cause>\
         <description>虚拟机处于无效状态。许多原因都可以导致虚拟机进入无效状态，例如无法访问数据存储。 </description>\
         <action>\
            确定虚拟机进入无效状态的原因，更正问题，然后重试该操作。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToPowerOnEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOnEvent">\
      <description>\
         无法打开虚拟机电源\
      </description>\
      <cause>\
         <description>\
            由于虚拟机已处于打开电源状态、虚拟机上正在运行并发操作等原因，尝试打开虚拟机电源的操作可能会失败。 </description>\
         <action>\
	  检查事件消息中的原因，找到打开电源失败的相应原因并解决该问题。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToRebootGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToRebootGuestEvent">\
      <description>\
         无法重新引导虚拟机上的客户机操作系统。\
      </description>\
      <cause>\
         <description>\
            由于虚拟机未处于打开电源状态、虚拟机上正在运行并发操作等原因，重新引导客户机操作系统可能会失败。\
         </description>\
	 <action>\
	  检查事件消息中的原因，找到重新引导失败的相应原因并解决该问题。\
         </action>\
       </cause>\
   </EventLongDescription>\
   "

VmFailedToResetEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToResetEvent">\
      <description>无法重置虚拟机</description>\
      <cause>\
         <description>\
		虚拟机可能正在等待对问题或提示的响应</description>\
         <action>\
            转至 vSphere Client 中虚拟机的“摘要”选项卡，然后对问题或提示进行响应\
         </action>\
      </cause>\
      <cause>\
         <description>可能没有足够的可用许可证来执行此操作。</description>\
         <action>\
            获取所需的许可证，然后重试重置操作\
         </action>\
      </cause>\
      <cause>\
         <description>\
            虚拟机上可能正在执行并发操作\
         </description>\
         <action>完成并发操作，然后重试重置操作</action>\
      </cause>\
      <cause>\
         <description>\
            正在运行虚拟机的主机正在进入维护模式\
         </description>\
         <action>\
            等待主机退出维护模式，然后重试此操作\
         </action>\
      </cause>\
      <cause>\
         <description>虚拟机处于无效状态。许多原因都可以导致虚拟机进入无效状态，例如无法访问数据存储。</description>\
         <action>\
            确定虚拟机进入无效状态的原因，更正问题，然后重试该操作。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

