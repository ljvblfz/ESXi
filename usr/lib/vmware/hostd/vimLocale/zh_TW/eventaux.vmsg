DatastoreDiscoveredEvent.longDescription = "\
  <EventLongDescription id="vim.event.DatastoreDiscoveredEvent">\
     <description>\
        在主機上探索到資料存放區\
     </description>\
     <cause>\
        <description>\
           已將有權存取此資料存放區的主機新增至資料中心\
        </description>\
     </cause>\
     <cause>\
        <description>\
           已向資料中心的主機取消遮罩支援此資料存放區的儲存空間\
        </description>\
     </cause>\
     <cause>\
        <description>\
           使用者或系統動作導致在主機上建立此資料存放區\
        </description>\
     </cause>\
     <cause>\
        <description>\
           使用者或系統動作導致在主機上建立此資料存放區，在此作業之前，在資料中心的至少一台其他主機上可以看到此資料存放區。 \
        </description>\
     </cause>\
   </EventLongDescription>\
   "

DatastoreRenamedOnHostEvent.longDescription = "\
   <EventLongDescription id="vim.event.DatastoreRenamedOnHostEvent">\
      <description>\
         已重新命名 vCenter Server 所管理主機上的資料存放區\
      </description>\
      <cause>\
         <description>\
            vCenter Server 在主機上探索到資料存放區並將其重新命名，因為此資料存放區已經以不同名稱存在於 vCenter Server 詳細目錄中。vCenter Server 也可能因為該名稱與同一個資料中心內另一個資料存放區的名稱相衝突而重新命名此資料存放區。 \
         </description>\
      </cause>\
   </EventLongDescription>\
  "

VMFSDatastoreExtendedEvent.longDescription = "\
