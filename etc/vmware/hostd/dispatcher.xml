       <matchCriterionList>
          <criterion id="1">
             <tagName>IsHostProfilesConfigThrottledOp</tagName>
            <value>1</value>
          </criterion>
       </matchCriterionList>
       <limit>1</limit>
     </policy>
     <policy id="LimitHostProfilesEditThrottledOp">
