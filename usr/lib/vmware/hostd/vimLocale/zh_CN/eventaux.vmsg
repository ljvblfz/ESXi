     "

VmFailedRelayoutOnVmfs2DatastoreEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedRelayoutOnVmfs2DatastoreEvent">\
        <description>\
           无法在 VMFS2 数据存储上迁移虚拟机\
        </description>\
        <cause>\
           <description>\
                由于虚拟机仍在 VMFS2 数据存储上具有磁盘，试图迁移虚拟机失败。对于 ESX 3.0 和更高版本的主机，VMFS2 数据存储是只读的。\
           </description>\
           <action> \
                将数据存储从 VMFS2 升级到 VMFS3\
           </action> \
        </cause>\
     </EventLongDescription>\
     "

VmFailedRelayoutEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedRelayoutEvent">\
        <description>\
           无法设置虚拟机布局 \
        </description>\
        <cause>\
           <description>\
                由于事件消息中的所述原因，试图在磁盘上设置虚拟机布局失败。由于任意几种原因 (例如无法访问磁盘)，可能会出现此情况。\
           </description>\
        <action>\
	    检查事件消息中的原因，找到失败的相应原因并解决该问题。\
         </action>\
	</cause>\
     </EventLongDescription>\
     "

VmAcquiredMksTicketEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmAcquiredMksTicketEvent">\
        <description>\
           已成功获取虚拟机的 MKS 票证\
        </description>\
        <cause>\
           <description>\
                已成功获取用于连接到虚拟机远程控制台的 MKS 票证。\
           </description>\
        </cause>\
     </EventLongDescription>\
     "

VmWwnAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnAssignedEvent">\
      <description>\
         为虚拟机分配了 WWN (万维网名称)\
      </description>\
      <cause>\
         <description>由于虚拟机是使用 RDM (裸设备映射) 磁盘创建的或重新配置为访问 RDM 磁盘，因此为虚拟机分配了 WWN\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmWwnChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnChangedEvent">\
      <description>\
         已更改为虚拟机分配的 WWN (万维网名称) \
      </description>\
      <cause>\
         <description>为该虚拟机分配了一个新的 WWN，这可能是由于为另一个虚拟机分配了相同 WWN 而导致冲突\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmWwnConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnConflictEvent">\
      <description>\
         为虚拟机分配的 WWN (万维网名称) 存在冲突 \
      </description>\
      <cause>\
         <description>为该虚拟机分配的 WWN 与其他虚拟机的 WWN 相同。\
       </description>\
      <action>\
	    检查事件详细信息，了解有关冲突的更多信息并解决该问题。\
         </action>\</cause>\
   </EventLongDescription>\
   "

VmReloadFromPathFailedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmReloadFromPathFailedEvent">\
      <description>\
         从新的数据存储路径重新加载虚拟机失败 \
      </description>\
      <cause>\
         <description>目标数据存储路径无法访问或无效\
         </description>\
         <action>使用有效的目标数据存储路径 \
         </action> \
      </cause>\
      <cause>\
         <description>虚拟机处于无效状态 \
         </description>\
         <action>检查虚拟机的电源状况。如果虚拟机已打开电源，请关闭其电源 \
         </action>\
      </cause>\
      <cause>\
         <description>为虚拟机启用了 Fault Tolerance\
         </description>\
         <action>禁用虚拟机的 Fault Tolerance，然后重试该操作 \
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationLinuxIdentityFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationLinuxIdentityFailed">\
      <description>\
         自定义脚本不支持客户机操作系统 Linux 分发。有关所支持的 Linux 分发的列表，请参见《VMware vSphere 兼容性列表》。\
      </description>\
      <cause>\
         <description>\
            不支持自定义目标客户机操作系统 Linux 分发。\
         </description>\
         <action>\
            咨询 VMware 了解何时会支持特定的 Linux 分发。如果更新版本已支持 Linux 分发，请考虑升级。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationNetworkSetupFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationNetworkSetupFailed">\
      <description>\
         自定义脚本无法在对应的 Linux 配置文件或 Windows 注册表中设置参数\
      </description>\
      <cause>\
         <description>\
            自定义规范包含无效的主机名或域名\
         </description>\
         <action>\
            查看此事件的客户机操作系统日志文件，了解更多详细信息\
         </action>\
         <action>\
            为目标客户机操作系统提供有效的主机名。该名称必须符合 RFC 952、1035、1123、2181 中的主机名和域名定义。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            找不到在自定义数据包中指定了 MAC 地址的 NIC \
         </description>\
         <action>\
            查看此事件的客户机操作系统日志文件，了解更多详细信息\
         </action>\
         <action>\
            确认在创建和部署自定义数据包期间没有更改虚拟机 NIC MAC 地址。调度自定义数据包后，在首次引导虚拟机时会进行部署。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            自定义代码需要某些配置文件的读/写权限。这些权限没有授予 Linux 上的“root”帐户，也没有授予 Windows 客户机操作系统上 VMware Tools Service 使用的帐户。\
         </description>\
         <action>\
            查看此事件的客户机操作系统日志文件，了解更多详细信息\
         </action>\
         <action>\
            将读/写权限授予 Linux 的“root”帐户，或授予 Windows 客户机操作系统上 VMware Tools Service 使用的帐户以及需要由自定义代码修改的注册表项\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationSysprepFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationSysprepFailed">\
      <description>\
         与选定的目标客户机操作系统对应的文件夹中的 sysprep 文件和该客户机操作系统的实际版本不兼容\
      </description>\
      <cause>\
         <description>\
            与目标客户机操作系统 (例如 Windows XP) 对应的文件夹中的 sysprep 文件适用于其他客户机操作系统 (例如 Windows 2003)\
         </description>\
         <action>\
            在运行 vCenter Server 的计算机上，将正确的 sysprep 文件放在与目标客户机操作系统对应的文件夹中\
         </action>\
      </cause>\
      <cause>\
         <description>\
            与客户机操作系统对应的文件夹中的 sysprep 文件适用于其他 Service Pack，例如客户机操作系统是 Windows XP SP2，而 sysprep 文件适用于 Windows XP SP1。\
         </description>\
         <action>\
            在运行 vCenter Server 的计算机上，将正确的 sysprep 文件放在与目标客户机操作系统对应的文件夹中\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationUnknownFailure.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationUnknownFailure">\
      <description>\
         自定义组件无法在客户机操作系统内设置必需的参数\
      </description>\
      <cause>\
         <description>\
            在 Windows 上，用来运行自定义代码的用户帐户对自定义代码使用的注册表项没有读/写权限。自定义代码通常是使用“本地系统”帐户运行的，但您可以通过为 VMware Tools Service 的执行选择其他帐户更改此帐户。\
         </description>\
         <action>\
            查看此事件的客户机操作系统日志文件，了解更多详细信息\
         </action>\
         <action>\
            确定为 VMware Tools Service 执行选择了哪个用户帐户，以及确认该帐户对自定义代码使用的注册表项是否拥有读/写权限\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Windows 上，用来运行自定义代码的用户帐户对自定义代码使用的文件和文件夹没有读/写权限。自定义代码通常是使用“本地系统”帐户运行的，但您可以通过为 VMware Tools Service 的执行选择其他帐户更改此帐户。\
         </description>\
         <action>\
            查看此事件的客户机操作系统日志文件，了解更多详细信息\
         </action>\
         <action>\
            确定为 VMware Tools Service 执行选择了哪个用户帐户，以及确认该帐户对自定义代码使用的文件和文件夹是否拥有读/写权限\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Linux 上，向自定义脚本传递了无效或不支持的时区，导致时区配置失败\
         </description>\
         <action>\
            查看此事件的客户机操作系统日志文件，了解更多详细信息\
         </action>\
         <action>\
            确认在自定义规范中传递了支持的时区。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Linux 上，客户机操作系统“root”帐户对自定义脚本需要修改的配置文件 (“/etc/hosts”) 没有读/写权限\
         </description>\
         <action>\
            为客户机操作系统“root”帐户授予对这些配置文件的读/写权限\
         </action>\
      </cause>\
      <cause>\
         <description>\
            在 Linux 上，如果使用 open-vm-tools，则还必须安装 deployPkg 插件，才能启用客户机自定义。\
         </description>\
         <action>\
            按照 kb.vmware.com/kb/2075048 的说明安装 open-vm-tools deployPkg 插件。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            不支持自定义目标客户机操作系统\
         </description>\
         <action>\
            咨询 VMware 了解何时会支持特定的 Linux 分发。如果更新版本已支持 Linux 分发，请考虑升级。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "
#
# FT Related Events
#

VmFailedStartingSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedStartingSecondaryEvent">\
       <description>\
          vCenter Server 无法启动辅助虚拟机，因为出现错误\
       </description>\
       <cause>\
            <description>\
              远程主机与辅助虚拟机不兼容。例如，当主机没有权限访问虚拟机的网络或数据存储时，便可能出现此情况。\
            </description>\
            <action>确保群集中的主机与 FT 兼容</action>\
        </cause>\
        <cause>\
            <description>登录到远程主机失败。如果远程主机刚添加到清单中或重新引导，则可能需要一些时间将 SSL 指纹传播到这些主机。\
            </description>\
            <action>如果问题仍然存在，请断开并重新连接该主机。</action>\
        </cause>\
        <cause>\
            <description>在远程主机上注册辅助虚拟机失败</description>\
            <action>确定远程主机是否有权访问 FT 虚拟机所在的数据存储</action>\
        </cause>\
        <cause>\
            <description>启动辅助虚拟机时出错</description>\
            <action>确定迁移错误的原因。vCenter Server 将在可能的情况下尝试重新启动辅助虚拟机。</action>\
        </cause>\
    </EventLongDescription>\
    "

VmFailedUpdatingSecondaryConfig.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedUpdatingSecondaryConfig">\
       <description>\
          在故障切换后，新的主虚拟机无法更新辅助虚拟机的配置\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "

VmFaultToleranceStateChangedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceStateChangedEvent">\
       <description>\
          已更改虚拟机的 Fault Tolerance 状况\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "

VmFaultToleranceTurnedOffEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceTurnedOffEvent">\
       <description>\
          已移除所有辅助虚拟机，并且关闭了此虚拟机的 Fault Tolerance 保护。\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "


VmFaultToleranceVmTerminatedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceVmTerminatedEvent">\
       <description>\
          主虚拟机或辅助虚拟机变为不活动状态\
       </description>\
       <cause>\
            <description> 辅助虚拟机变为不活动状态，因为其操作不再与主虚拟机的操作同步</description>\
            <action>vSphere HA 将尝试重新启动辅助虚拟机</action>\
        </cause>\
       <cause>\
            <description> 辅助虚拟机变为不活动状态，因为硬件或网络故障导致主虚拟机丢失与辅助虚拟机的连接</description>\
            <action>vSphere HA 将尝试重新启动辅助虚拟机</action>\
        </cause>\
       <cause>\
            <description> 由于物理主机上的部分硬件出现故障，Fault Tolerant 虚拟机变为不活动状态</description>\
            <action>vSphere HA 将尝试重新启动辅助虚拟机</action>\
        </cause>\
       <cause>\
            <description> 用户停止了 Fault Tolerant 虚拟机</description>\
            <action>其余的 Fault Tolerant 虚拟机会作为主虚拟机进行接管。vSphere HA 将尝试重新启动辅助虚拟机。</action>\
        </cause>\
    </EventLongDescription>\
    "


VmMaxFTRestartCountReached.longDescription = "\
    <EventLongDescription id="vim.event.VmMaxFTRestartCountReached">\
       <description>\
          系统在尝试重新启动辅助虚拟机时达到最大重新启动限值\
       </description>\
       <cause>\
            <description>系统在尝试重新建立 Fault Tolerance 时，超出了允许的辅助虚拟机重新启动尝试次数</description>\
            <action>检查重新启动故障原因并进行修复。然后，禁用并重新启用 Fault Tolerance 保护。</action>\
        </cause>\
    </EventLongDescription>\
    "

VmNoCompatibleHostForSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmNoCompatibleHostForSecondaryEvent">\
       <description>\
          尝试放置辅助虚拟机时找不到兼容主机\
       </description>\
       <cause>\
            <description>没有可用于放置辅助虚拟机的兼容主机</description>\
            <action>解决不兼容性问题，然后重试该操作</action>\
        </cause>\
    </EventLongDescription>\
    "

VmSecondaryDisabledBySystemEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmSecondaryDisabledBySystemEvent">\
       <description>\
          由于无法打开辅助虚拟机电源，vCenter Server 已将辅助虚拟机禁用\
       </description>\
       <cause>\
            <description>vCenter Server 无法打开辅助虚拟机的电源 </description>\
            <action>检查事件消息中的原因了解更多详细信息，修复故障，然后重新启用 Fault Tolerance 保护以打开辅助虚拟机电源。</action>\
        </cause>\
    </EventLongDescription>\
    "


VmTimedoutStartingSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmTimedoutStartingSecondaryEvent">\
       <description> 尝试启动辅助虚拟机超时。\
       </description>\
       <cause>\
            <description>用户尝试打开或启用 Fault Tolerance，导致辅助虚拟机启动。启动操作超时，因此 vCenter Server 禁用了 Fault Tolerance。 </description>\
            <action>解决任何问题并重新启用 Fault Tolerance 保护</action>\
        </cause>\
       <cause>\
            <description>辅助虚拟机已启动以响应故障，但启动尝试超时</description>\
            <action> vSphere HA 将尝试打开辅助虚拟机的电源</action>\
        </cause>\
    </EventLongDescription>\
    "

CanceledHostOperationEvent.longDescription = "\
	<EventLongDescription id="vim.event.CanceledHostOperationEvent">\
