DrsExitStandbyModeFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.DrsExitStandbyModeFailedEvent">\
       <description>\
          DPM n'a pas mis un hôte dans le mode de veille.  DPM a essayé de mettre un hôte sous tension en utilisant IPMI, iLO ou le protocole LAN d'éveil par appel réseau, mais l'hôte  n'a pas été mis sous tension.\
       </description>\
       <cause>\
            <description>DPM n'a pas pu communiquer avec le BMC sur l'hôte</description>\
            <action>Vérifiez les qualifications d'IPMI/iLO présentées dans vCenter Server</action>\
            <action>Vérifiez que l'accès LAN est activé dans le BMC</action>\
        </cause>\
        <cause>\
            <description>le NIC de vMotion sur l'hôte ne prend pas en charge l'éveil par appel réseau LAN</description>\
            <action>Sélectiononez un NIC vMotion qui prend en charge l'éveil par appel réseau LAN</action>\
        </cause>\
    </EventLongDescription>\
    "

ClusterOvercommittedEvent.longDescription = "\
    <EventLongDescription id="vim.event.ClusterOvercommittedEvent">\
       <description>\
          Les ressources cumulatives CPU et/ou de mémoire de tous les hôtes dans le cluster  ne sont pas adéquates pour satisfaire les réservations de ressources de toutes les machines virtuelles dans le  cluster\
       </description>\
       <cause>\
            <description>Vous avez essayé de mettre sous tension une machine virtuelle en contournant vCenter Server.  Cette condition se produit quand vous tentez de mettre sous tension en utilisant vSphere Client directement  connecté à l'hôte.</description>\
            <action>Dans un cluster DRS, ne mettez pas sous tension de machines virtuelles en contournant vCenter Server</action>\
        </cause>\
        <cause>\
            <description>Un hôte a été placé en mode maintenance, veille ou déconnexion</description>\
            <action>Cessez tout mode d'hôte en mode maintenance, veille ou déconnexion</action>\
        </cause>\
    </EventLongDescription>\
    "

ResourceViolatedEvent.longDescription = "\
    <EventLongDescription id="vim.event.ResourceViolatedEvent">\
       <description>\
          La consommation cumulative de ressource en CPU et/ou en mémoire de toutes les machines virtuelles dans le  pool de ressources dépasse la configuration de pool de ressources \
       </description>\
       <cause>\
            <description>Vous avez essayé de faire passer une machine virtuelle d'un pool de ressources à un autre  en contournant vCenter Server. Cette condition se produit quand vous essayez d'utiliser  vSphere Client directement connecté à l'hôte.\
		</description>\
            <action>Dans un cluster DRS, ne déplacez pas, ni ne mettez sous tension une machine virtuelle en contournant vCenter Server</action>\
        </cause>\
    </EventLongDescription>\
    "

HostComplianceCheckedEvent.longDescription = "\
   <EventLongDescription id="vim.event.HostComplianceCheckedEvent">\
      <description>\
         L'hôte a été examiné pour assurer la conformité avec un profil d'hôte\
      </description>\
