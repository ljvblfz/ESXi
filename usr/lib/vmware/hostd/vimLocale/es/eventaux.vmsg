GhostDvsProxySwitchDetectedEvent.longDescription = "\
    <EventLongDescription id="vim.event.GhostDvsProxySwitchDetectedEvent">\
       <description> \
          vCenter Server encontró un conmutador proxy vSphere Distributed Switch en el host que no coincide con ningún conmutador \
 vSphere Distributed Switch de vCenter Server. \
       </description>\
       <cause>\
            <description> \
               El conmutador vSphere Distributed Switch correspondiente al conmutador proxy vSphere Distributed Switch \
 en el host se eliminó cuando el host se desconectó de vCenter Server.\
            </description>\
        </cause>\
       <cause>\
            <description> \
               El host ya no es miembro del conmutador vSphere Distributed Switch al que corresponde el conmutador proxy \
 del host. \
            </description>\
        </cause>\
    </EventLongDescription>\
    "

IncorrectHostInformationEvent.longDescription = "\
