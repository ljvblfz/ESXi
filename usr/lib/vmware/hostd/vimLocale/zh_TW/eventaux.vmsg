HostInventoryFullEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostInventoryFullEvent">\
       <description>\
          vCenter Server Foundation 授權金鑰目前只允許將三台主機新增至詳細目錄。新增額外主機會導致發生錯誤並記錄此事件。 \
       </description>\
       <cause>\
            <description>正在嘗試新增的主機數目超過指派給 vCenter Server 之授權金鑰所允許的數目</description>\
            <action>將允許更多主機或無主機數目限制的授權金鑰指派給 vCenter Server</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseNonComplianceEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseNonComplianceEvent">\
       <description>\
          vCenter Server 不會嚴格強制執行授權使用率，而是會定期檢查授權超用情況。如果 vCenter Server 偵測到超用情況，則會記錄此事件並觸發警示。\
       </description>\
       <cause>\
            <description>授權超用</description>\
            <action>透過 vSphere Client 查看授權報告，並減少使用授權金鑰的實體數目，或者新增容量更大的授權金鑰</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseServerUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseServerUnavailableEvent">\
       <description>\
          如果授權伺服器已停止回應，則 vCenter Server 會追蹤授權伺服器狀態並記錄此事件。 \
       </description>\
       <cause>\
            <description>授權伺服器沒有回應，無法供 vCenter Server 使用</description>\
            <action>確認授權伺服器是否正在執行中。若是，請檢查 vCenter Server 與授權伺服器之間的連線。</action>\
       </cause>\
    </EventLongDescription>\
    "

NoLicenseEvent.longDescription = "\
    <EventLongDescription id="vim.event.NoLicenseEvent">\
       <description>\
          如果由於未知原因而導致 vCenter Server 無法從授權伺服器取得功能，則 vCenter Server 會記錄此事件。 \
       </description>\
       <cause>\
            <description>未知原因導致取得功能授權失敗</description>\
            <action>確認授權伺服器是否具有該功能的授權</action>\
       </cause>\
    </EventLongDescription>\
    "


InvalidEditionEvent.longDescription = "\
    <EventLongDescription id="vim.event.InvalidEditionEvent">\
       <description>\
          vCenter Server 嘗試從授權伺服器取得未定義的功能 \
       </description>\
       <cause>\
            <description>如果未在授權伺服器上定義功能，則需要提供功能授權 (如 vMotion、DRS、vSphere HA) 的任何作業都可能導致發生此事件</description>\
            <action>確認授權伺服器上是否存在所涉及的功能</action>\
       </cause>\
    </EventLongDescription>\
    "

HostLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostLicenseExpiredEvent">\
       <description>\
          vCenter Server 會追蹤授權伺服器上主機授權的到期時間，並使用此事件來通知您即將到期的任何主機授權\
       </description>\
       <cause>\
            <description>授權伺服器上的主機授權即將到期</description>\
            <action>更新授權伺服器以取得新版本的主機授權</action>\
       </cause>\
    </EventLongDescription>\
    "

VMotionLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.VMotionLicenseExpiredEvent">\
       <description>\
          vCenter Server 會追蹤授權伺服器上 vMotion 授權的到期時間，並使用此事件來通知您即將到期的任何 vMotion 授權\
       </description>\
       <cause>\
            <description>授權伺服器上的 vMotion 授權即將到期</description>\
            <action>更新授權伺服器以取得較新版本的 vMotion 授權</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseRestrictedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseRestrictedEvent">\
       <description>\
          如果因授權檔案限制而導致 vCenter Server 無法從授權伺服器簽出授權，則 vCenter Server 會記錄此事件 \
       </description>\
       <cause>\
            <description>授權伺服器中的授權檔案具有禁止簽出的限制</description>\
            <action>檢查授權檔案，並移除任何可移除的限制</action>\
       </cause>\
    </EventLongDescription>\
    "

VmFailedToPowerOffEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOffEvent">\
      <description>虛擬機器無法關閉電源</description>\
      <cause>\
         <description>\
            虛擬機器可能正在執行並行作業\
         </description>\
         <action>完成並行作業，然後重試關閉電源作業</action>\
      </cause>\
      <cause>\
         <description>虛擬機器處於無效狀態。導致虛擬機器進入無效狀態的原因有多種，例如，無法存取資料存放區。 </description>\
         <action>\
            確定虛擬機器進入無效狀態的原因，並更正問題，然後重試該作業。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToPowerOnEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOnEvent">\
      <description>\
         虛擬機器無法開啟電源\
      </description>\
      <cause>\
         <description>\
            虛擬機器開啟電源嘗試失敗的原因可能包括: 虛擬機器已處於開啟電源狀態、虛擬機器正在執行並行作業等。 </description>\
         <action>\
	  在事件訊息中查看原因，以找出開啟電源失敗的原因並修正此問題。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToRebootGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToRebootGuestEvent">\
      <description>\
         虛擬機器上的客體作業系統無法重新啟動。\
      </description>\
      <cause>\
         <description>\
            發生客體作業系統重新啟動失敗的原因可能包括: 虛擬機器未處於開啟電源狀態、虛擬機器正在執行並行作業等。\
         </description>\
	 <action>\
	  在事件訊息中查看原因，以找出重新啟動失敗的原因並修正此問題。\
         </action>\
       </cause>\
   </EventLongDescription>\
   "

VmFailedToResetEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToResetEvent">\
      <description>虛擬機器無法重設</description>\
      <cause>\
         <description>\
		虛擬機器可能正在等待問題或提示的回應</description>\
         <action>\
            移至 vSphere Client 中虛擬機器的「摘要」索引標籤，然後回應問題或提示\
         </action>\
      </cause>\
      <cause>\
         <description>可能沒有足夠的可用授權來執行此作業。</description>\
         <action>\
            取得必要授權，然後重試重設作業\
         </action>\
      </cause>\
      <cause>\
         <description>\
            虛擬機器可能正在執行並行作業\
         </description>\
         <action>完成並行作業，然後重試重設作業</action>\
      </cause>\
      <cause>\
         <description>\
            正在執行虛擬機器的主機即將進入維護模式\
         </description>\
         <action>\
            等待主機結束維護模式，然後重試該作業\
         </action>\
      </cause>\
      <cause>\
         <description>虛擬機器處於無效狀態。導致虛擬機器進入無效狀態的原因有多種，例如，無法存取資料存放區。</description>\
         <action>\
            確定虛擬機器進入無效狀態的原因，並更正問題，然後重試該作業。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

