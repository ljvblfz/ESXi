HostInventoryFullEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostInventoryFullEvent">\
       <description>\
          La clave de licencia de vCenter Server Foundation actualmente permite agregar \
 solo tres hosts al inventario. Si se agregan hosts adiciones, se producen \
 errores y se registra este evento. \
       </description>\
       <cause>\
            <description>Intentando agregar más hosts que la cantidad permitida por la clave \
 de licencia asignada a vCenter Server</description>\
            <action>Asignar una clave de licencia de vCenter Server que permita más hosts \
 o que no tenga límite de hosts</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseNonComplianceEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseNonComplianceEvent">\
       <description>\
          vCenter Server no aplica estrictamente el uso de la licencia. En su lugar, comprueba periódicamente \
 el uso excesivo de la licencia. Si vCenter Server detecta un uso excesivo, registra este evento y \
 activa una alarma.\
       </description>\
       <cause>\
            <description>Uso excesivo de licencias</description>\
            <action>Compruebe los informes de licencias por medio de vSphere Client y reduzca la cantidad de \
 entidades utilizando la clave de licencia o agregue una nueva clave de licencia \
 con más capacidad.</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseServerUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseServerUnavailableEvent">\
       <description>\
          vCenter Server realiza un seguimiento del estado del servidor de licencias y registra este evento si \
 el servidor de licencias ha dejado de responder. \
       </description>\
       <cause>\
            <description>El servidor de licencias no responde y no está disponible para vCenter Server</description>\
            <action>Verifique que el servidor de licencias se esté ejecutando. Si lo está, compruebe la conectividad entre \
 vCenter Server y el servidor de licencias.</action>\
       </cause>\
    </EventLongDescription>\
    "

NoLicenseEvent.longDescription = "\
    <EventLongDescription id="vim.event.NoLicenseEvent">\
       <description>\
          vCenter Server registra este evento si no puede adquirir una característica del servidor de licencias debido a un \
 motivo desconocido. \
       </description>\
       <cause>\
            <description>Se produce un error en la adquisición de una licencia de características debido a un motivo desconocido.</description>\
            <action>Verifique que el servidor de licencias cuente con la licencia para la característica.</action>\
       </cause>\
    </EventLongDescription>\
    "


InvalidEditionEvent.longDescription = "\
    <EventLongDescription id="vim.event.InvalidEditionEvent">\
       <description>\
          vCenter Server intentó adquirir una característica no definida desde el servidor de licencias. \
       </description>\
       <cause>\
            <description>Cualquier operación que requiera una licencia de características como vMotion, DRS o vSphere HA puede generar \
 este evento si la característica no se ha definido en el servidor de licencias.</description>\
            <action>Verifique que la característica en cuestión esté presente en el servidor de licencias.</action>\
       </cause>\
    </EventLongDescription>\
    "

HostLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostLicenseExpiredEvent">\
       <description>\
          vCenter Server realiza un seguimiento de las horas de caducidad de las licencias de host en el servidor de licencias \
 y utiliza este evento para notificarle sobre las licencias de host que estén a punto de caducar.\
       </description>\
       <cause>\
            <description>Las licencias de host del servidor de licencias están a punto a caducar.</description>\
            <action>Actualizar el servidor de licencias para obtener una nueva versión de la licencia de host</action>\
       </cause>\
    </EventLongDescription>\
    "

VMotionLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.VMotionLicenseExpiredEvent">\
       <description>\
          vCenter Server realiza un seguimiento de las horas de caducidad de las licencias de vMotion en el servidor de licencias \
 y utiliza este evento para notificarle sobre las licencias de vMotion cuya hora de caducidad de aproxima\
       </description>\
       <cause>\
            <description>Las licencias de vMotion en el servidor de licencias están próximas a caducar</description>\
            <action>Actualizar el servidor de licencias para obtener una nueva versión de la licencia de vMotion</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseRestrictedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseRestrictedEvent">\
       <description>\
          vCenter Server realiza un seguimiento de este evento si no puede comprobar una licencia desde el \
 servidor de licencias debido a restricciones en el archivo de licencia \
       </description>\
       <cause>\
            <description>El archivo de licencia en el servidor de licencias tiene restricciones que impiden la comprobación</description>\
            <action>Compruebe el archivo de licencia y quite las restricciones que pueda</action>\
       </cause>\
    </EventLongDescription>\
    "

VmFailedToPowerOffEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOffEvent">\
      <description>No se pudo apagar la máquina virtual</description>\
      <cause>\
         <description>\
            Es posible que la máquina virtual esté ejecutando operaciones simultáneas\
         </description>\
         <action>Finalice las operaciones simultáneas y vuelva a intentar la operación de apagado</action>\
      </cause>\
      <cause>\
         <description>La máquina virtual está en un estado no válido. Las máquinas virtuales pueden entrar \
 en un estado no válido por diversos motivos, por ejemplo, la inaccesibilidad del almacén de datos. </description>\
         <action>\
            Identifique el motivo por el que la máquina virtual entró en un estado no válido, corrija el problema y \
 vuelva a intentar la operación.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToPowerOnEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOnEvent">\
      <description>\
         No se pudo encender la máquina virtual\
      </description>\
      <cause>\
         <description>\
            Pueden producirse errores en los intentos de encendido de una máquina virtual debido a que la máquina virtual ya está en \
 estado encendido, porque se están ejecutando operaciones simultáneas en la máquina virtual, etc. </description>\
         <action>\
	  Compruebe el motivo en el mensaje del evento para averiguar la causa del error de encendido y corrija el problema.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToRebootGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToRebootGuestEvent">\
      <description>\
         El sistema operativo invitado en la máquina virtual no pudo reiniciarse.\
      </description>\
      <cause>\
         <description>\
            Los errores de reinicio de los sistemas operativos invitados pueden producirse debido a que la máquina virtual no está en \
 estado encendido, porque se están ejecutando operaciones simultáneas en la máquina virtual, etc.\
         </description>\
	 <action>\
	  Compruebe el motivo en el mensaje del evento para averiguar la causa del error de reinicio y corrija el problema.\
         </action>\
       </cause>\
   </EventLongDescription>\
   "

VmFailedToResetEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToResetEvent">\
      <description>No se pudo restablecer la máquina virtual</description>\
      <cause>\
         <description>\
		Es posible que la máquina virtual esté esperando una respuesta a una pregunta o una indicación</description>\
         <action>\
            Vaya a la pestaña Resumen para la máquina virtual en vSphere Client y responda la pregunta o la indicación\
         </action>\
      </cause>\
      <cause>\
         <description>Es posible que no haya suficientes licencias disponibles para realizar esta operación.</description>\
         <action>\
            Obtenga las licencias necesarias y vuelva a intentar la operación de restablecimiento\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Es posible que se estén ejecutando operaciones simultáneas en la máquina virtual\
         </description>\
         <action>Finalice las operaciones simultáneas y vuelva a intentar la operación de restablecimiento</action>\
      </cause>\
      <cause>\
         <description>\
            El host en el que se ejecuta la máquina virtual está entrando en modo de mantenimiento\
         </description>\
         <action>\
            Espere hasta que el host salga del modo de mantenimiento y vuelva a intentar la operación\
         </action>\
      </cause>\
      <cause>\
         <description>La máquina virtual está en un estado no válido. Las máquinas virtuales pueden entrar \
 en un estado no válido por diversos motivos, por ejemplo, la inaccesibilidad del almacén de datos.</description>\
         <action>\
            Identifique el motivo por el que la máquina virtual entró en un estado no válido, corrija el problema y \
 vuelva a intentar la operación.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmUpgradingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUpgradingEvent">\
      <description>El hardware virtual de esta máquina virtual se está actualizado</description>\
      <cause>\
         <description>Una acción iniciada por el usuario activó una actualización en el hardware de la máquina virtual</description>\
      </cause>\
      <cause>\
         <description>Una tarea programada inició una actualización del hardware de la máquina virtual</description>\
      </cause>\
   </EventLongDescription>\
   "

VmUuidAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUuidAssignedEvent">\
      <description>Se asignó un nuevo UUID del BIOS a la máquina virtual</description>\
      <cause>\
         <description>El usuario que creó la máquina virtual no especificó un UUID del BIOS en el momento de la creación. \
            vCenter Server generó un nuevo UUID y lo asignó a la máquina virtual.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidAssignedEvent">\
      <description>Se asignó un nuevo UUID de instancia específico de vCenter Server a la máquina virtual.\
      </description>\
      <cause>\
         <description> \
             El usuario que creó la máquina virtual no especificó un UUID de instancia específico de vCenter Server en el \
 momento de la creación. vCenter Server generó un nuevo UUID y lo asignó a la \
 máquina virtual.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmUuidChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUuidChangedEvent">\
      <description>El UUID del BIOS de la máquina virtual ha cambiado</description>\
      <cause>\
         <description>\
            Un usuario cambió el UUID del BIOS de la máquina virtual directamente en el host.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidChangedEvent">\
      <description> \
         El UUID de instancia específico de vCenter Server de la máquina virtual ha cambiado.\
      </description> \
      <cause>\
         <description>\
            Una acción del usuario produjo un cambio en el UUID de instancia específico de vCenter Server de la \
 máquina virtual.\
         </description>\
      </cause>\
      <cause>\
         <description>\
            vCenter Server cambió el UUID de instancia de la máquina virtual porque se detectó \
 un conflicto.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidChangedEvent">\
      <description> \
         El UUID de instancia específico de vCenter Server de la máquina virtual entró en conflicto con el \
 de otra máquina virtual.. \
      </description>\
      <cause>\
         <description>\
             Los conflictos de los UUID de instancias de máquinas virtuales pueden producirse si se copian archivos \
 de máquinas virtuales manualmente sin usar vCenter Server. \
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmRelocateFailedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmRelocateFailedEvent">\
      <description>\
         Error al intentar reubicar la máquina virtual en otro host u otro almacén de datos\
      </description>\
      <cause>\
         <description>\
            Los errores al intentar reubicar una máquina virtual pueden deberse a varios motivos, \
 entre ellos, interrupciones de la red, espacio en disco insuficiente, etc.\
         </description>\
         <action>\
            Considere la tarea relacionada con este evento, evalúe el motivo del error y lleve a cabo la acción correspondiente.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmUnsupportedStartingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUnsupportedStartingEvent">\
      <description>\
         Intentando encender una máquina virtual que tiene un sistema operativo invitado no compatible\
      </description>\
      <cause>\
         <description>\
            Una acción del usuario inició una operación de encendido de una máquina virtual, pero la \
 máquina virtual tiene un sistema operativo invitado no compatible.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

# VmRegisteredEvent: Basic event has enough details
