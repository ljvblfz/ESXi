ClusterReconfiguredEvent.longDescription = "\
    <EventLongDescription id="vim.event.ClusterReconfiguredEvent">\
       <description>\
          群集配置已更改。群集配置包含有关群集的 DRS、DPM、EVC 和 vSphere HA 设置的信息。所有 DRS 规则也存储在群集配置中。编辑群集配置可能会在群集中的每个主机上触发调用 DRS 和/或启用/禁用 vSphere HA。 \
       </description>\
    </EventLongDescription>\
    "

ClusterStatusChangedEvent.longDescription = "\
    <EventLongDescription id="vim.event.ClusterStatusChangedEvent">\
       <description>\
          群集状态已更改。此状态是包含整个群集的 root 资源池的状态。群集状态更改可以排除配置问题 (如果先前检测到配置问题)。群集状态为绿色表示一切正常。黄色状态表示 root 资源池没有资源满足其子资源池的预留。红色状态表示资源池中某个节点的子节点的预留超出该节点的配置。 \
       </description>\
       <cause>\
            <description>群集状态更改为黄色</description>\
            <action>直接在 root 资源池下添加更多资源 (更多主机) 或减少资源池的预留，以便与新容量匹配</action>\
        </cause>\
        <cause>\
            <description>群集状态更改为红色</description>\
            <action>更改状态为红色的资源池的资源设置，以便这些资源池可以容纳其子虚拟机。如果无法更改设置，请减少虚拟机预留。如果无法减少虚拟机预留，请关闭部分虚拟机电源。</action>\
        </cause>\
    </EventLongDescription>\
    "

DrsInvocationFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.DrsInvocationFailedEvent">\
       <description>\
          无法成功完成 DRS 调用。出现这种情况有许多原因，有些原因可能是暂时性的。 \
       </description>\
       <cause>\
            <description>DRS 调用过程中遇到错误</description>\
            <action>禁用 DRS，然后重新启用它</action>\
        </cause>\
    </EventLongDescription>\
    "

DrsResourceConfigureFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.DrsResourceConfigureFailedEvent">\
       <description>\
          无法成功将 DRS 资源设置应用于群集中的主机。这种情况通常是暂时性的。 \
       </description>\
       <cause>\
            <description>无法将 DRS 资源设置应用于主机。</description>\
            <action>DRS 生成将群集值映射到主机的资源设置。但是，在这种情况下，无法将群集值成功应用于该主机。该错误通常是从 DRS 同步到主机时存在延迟所导致的暂时性错误。如果这种情况持续存在，请在 vpxa 中启用调试日志记录，并与 VMware 支持部门联系。 </action>\
        </cause>\
    </EventLongDescription>\
    "

DrsVmMigratedEvent.longDescription = "\
    <EventLongDescription id="vim.event.DrsVmMigratedEvent">\
       <description>\
          已根据 DRS 建议迁移虚拟机。给出此建议可能是为了更好地平衡群集中的负载，或者是为了撤出群集中正被置于待机或维护模式的主机。 \
       </description>\
       <cause>\
            <description>DRS 建议迁移虚拟机</description>\
        </cause>\
    </EventLongDescription>\
    "

DrsVmPoweredOnEvent.longDescription = "\
    <EventLongDescription id="vim.event.DrsVmPoweredOnEvent">\
       <description>\
          用户已打开虚拟机电源，并且 DRS 根据当前群集负载分布和虚拟机的资源要求为该虚拟机选择一台主机 \
       </description>\
       <cause>\
            <description>DRS 为正在打开电源的虚拟机选择了一台主机</description>\
        </cause>\
    </EventLongDescription>\
    "

HostOvercommittedEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostOvercommittedEvent">\
       <description>\
          主机的 CPU 和/或内存容量不足，无法满足其资源配置。该主机有自己的准入控制，因此这种情况决不会出现。\
       </description>\
       <cause>\
            <description>主机的容量不足，无法满足其资源配置</description>\
            <action>如果出现这种情况，请与 VMware 支持部门联系 </action>\
	</cause>\
    </EventLongDescription>\
    "

HostStatusChangedEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostStatusChangedEvent">\
       <description>\
          主机状态已更改。此状态是包含整个主机的 root 资源池的状态。主机状态更改可以排除配置问题 (如果先前检测到配置问题)。主机状态为绿色表示一切正常。黄色状态表示 root 资源池没有资源满足其子资源池的预留。红色状态表示资源池中某个节点的子节点的预留超出该节点的配置。 \
       </description>\
       <cause>\
            <description>主机状态更改为黄色</description>\
            <action>直接在 root 资源池下减少资源池的预留，以便与新容量匹配</action>\
        </cause>\
        <cause>\
            <description>主机状态更改为红色</description>\
            <action>更改状态为红色的资源池的资源设置，以便这些资源池可以容纳其子虚拟机。如果无法更改设置，请减少虚拟机预留。如果无法减少虚拟机预留，请关闭部分虚拟机电源。</action>\
        </cause>\
    </EventLongDescription>\
    "

NoMaintenanceModeDrsRecommendationForVM.longDescription = "\
    <EventLongDescription id="vim.event.NoMaintenanceModeDrsRecommendationForVM">\
       <description>\
          DRS 无法为进入维护模式的主机上的虚拟机生成 vMotion 建议。出现这种情况通常是因为 DRS 群集中没有其他主机与该虚拟机兼容。除非您手动迁移或关闭此虚拟机电源，否则该主机将无法进入维护模式。 \
       </description>\
       <cause>\
            <description>DRS 无法撤出已打开电源的虚拟机</description>\
            <action>手动将该虚拟机迁移到群集中的另一个主机</action>\
            <action>关闭虚拟机电源</action>\
            <action>使处于维护模式的所有主机退出该模式</action>\
            <action>取消正在使该主机进入维护模式的任务 </action>\
        </cause>\
    </EventLongDescription>\
    "

NotEnoughResourcesToStartVmEvent.longDescription = "\
    <EventLongDescription id="vim.event.NotEnoughResourcesToStartVmEvent">\
       <description>\
          当主代理无法将虚拟机故障切换到其中一台兼容主机时， \
	  vSphere HA 就会生成此事件。这种情况由下列一个或多个原因 \
	  引起。如果发生此情况，则 vSphere HA 将在事件摘要中报告 \
	  其原因，但请注意可能存在其他原因。\
	  如果 vSphere HA 准入控制被禁用，或者故障主机数超过之前 \
	  置备的主机数，则更可能出现这种情况。如果无法放置虚拟机， \
	  vSphere HA 将在群集状态发生更改时重新尝试进行放置。 \
	  另外，如果启用了 vSphere DRS，将调用它来尝试对群集进行 \
	  碎片整理或使主机退出待机模式。\
       </description>\
       <cause>\
            <description>\
            虚拟机为 CPU、内存、vFlash 缓存或虚拟网卡预留了带宽。\
	            没有兼容主机具备足够资源来满足 \
	            预留。\
            </description>\
            <action>减少虚拟机的资源预留</action>\
            <action>向群集中添加更多主机</action>\
            <action>将任何出现故障的主机联机，或者解析可能存在的网络分区</action>\
            <action>如果 DRS 处于手动模式，请查找所有待定建议并批准这些 \
            建议，以便 vSphere HA 故障切换可以继续执行</action>\
        </cause>\
        <cause>\
            <description>\
	         该群集已启用 vSAN，并且有一个或多个为群集提供存储\
               的主机无法访问，致使 vSphere HA 无法打开虚拟机电源。\
               在 vSAN 数据存储上拥有一个或多个文件的虚拟机会出现这种情况。\
            </description>\
            <action>将任何出现故障的主机联机，或者解析可能存在的、涉及到 \
            为 vSAN 群集提供存储的主机的网络分区</action>\
        </cause>\
        <cause>\
            <description>与虚拟机关联的一个或多个数据存储 \
               无法供群集中的任何兼容主机访问。</description>\
            <action>使挂载虚拟机数据存储的任何无响应主机联机</action>\
            <action>修复全部路径异常 (APD) 或永久设备丢失 (PDL) 问题。</action>\
        </cause>\
        <cause>\
            <description>vSphere HA 强制虚拟机执行虚拟机 \
               反关联性规则，而该规则未得到满足。 </description>\
            <action>向群集添加更多主机</action>\
            <action>将任何无响应主机联机，或者解析可能存在的网络分区</action>\
            <action>移除限制放置的任何反关联性规则</action>\
        </cause>\
        <cause>\
            <description>每个主机上可以运行的虚拟机数量存在限制。 \
               没有主机可以在不超过该限制的情况下打开虚拟机电源。</description>\
            <action>如果已设置 limitVmsPerESXHost HA 高级选项，请提高该限制值。</action>\
            <action>使任何无响应主机联机，或向群集添加新主机</action>\
        </cause>\
    </EventLongDescription>\
    "

ResourcePoolReconfiguredEvent.longDescription = "\
    <EventLongDescription id="vim.event.ResourcePoolReconfiguredEvent">\
       <description>\
          资源池配置已更改。资源池配置包含有关资源池及其子资源池的资源预留的信息。 \
       </description>\
    </EventLongDescription>\
    "

DasHostIsolatedEvent.longDescription = "\
    <EventLongDescription id="vim.event.DasHostIsolatedEvent">\
       <description>\
          vSphere HA 检测到主机处于网络隔离状态。当主机处于此状况时，vSphere HA 会对主机上运行的虚拟机应用关闭主机电源或主机关机隔离响应，然后继续监控保持打开电源状态的虚拟机。当主机处于此状况时，vSphere HA 在虚拟机出现故障后重新启动虚拟机的能力将受到影响。如果主机上的代理确定由主代理负责虚拟机，则 vSphere HA 仅对虚拟机执行关闭电源或关机。 \
       </description>\
       <cause>\
            <description> \
                如果满足以下两个条件，则主机处于网络隔离状态: (1) 隔离地址已配置，且主机无法 Ping 这些地址；(2) 主机上的 vSphere HA 代理无法访问其他群集主机上运行的任何代理。 \
            </description>\
            <action>\
                 解决阻止主机 Ping 其隔离地址并与其他主机通信的网络问题。确保 vSphere HA 所使用的管理网络中存在冗余。存在冗余时，vSphere HA 可以通过多条路径进行通信，从而减少主机隔离的几率。 \
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostMissingNetworksEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostMissingNetworksEvent">\
       <description>\
       要添加到 vSphere HA 群集中的主机的管理网络数少于群集中现有主机的管理网络数。为主机配置 vSphere HA 时，会针对群集中的现有主机，检查 vSphere HA 用于信号检测和其他 vSphere HA 通信的网络。加入主机要求具有相同的管理网络数，并且最好具有公共子网。这有助于对检测信号的源/目标对进行配对。如果在要添加的成员与群集中的现有成员之间检测不到公共子网 (使用 IP 地址/子网掩码)，将生成此事件并且配置任务会失败。事件详细信息将报告加入成员上没有的现有成员子网。 \
       </description>\
       <cause>\
            <description>\
                主机的网络与现有群集成员不兼容\
            </description>\
            <action>\
                更改主机的网络配置，以便在群集中的现有主机所在的子网中启用 vSphere HA 流量。在 ESX 上，vSphere HA 将使用“服务控制台”端口组；在 ESXi 主机上，vSphere HA 将使用选中了“管理流量”复选框的端口组。更改主机的网络配置后，请重新配置此主机的 vSphere HA。\
            </action>\
            <action>\
                使用高级选项替代选择用于 vSphere HA 群集通信的默认端口组。可以使用 das.allowNetwork[X] 高级选项告知 vSphere HA 使用此选项中指定的端口组。对于每个应该使用的端口组名称，指定一个 das.allowNetwork[X] 高级选项。vSphere HA 配置会检查要为与指定名称匹配的端口组添加的主机。配置任务还会检查其端口组与指定名称匹配的现有成员。每个主机上匹配的端口组名称数必须相同。设置高级选项后，请重新启用此群集的 vSphere HA。\
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostExtraNetworksEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostExtraNetworksEvent">\
       <description>\
       要添加到 vSphere HA 群集的主机的管理网络数大于群集中现有主机的管理网络数。为主机配置 vSphere HA 时，会针对群集中的现有主机，检查 vSphere HA 用于信号检测和其他 vSphere HA 通信的网络。加入主机要求具有相同的管理网络数，并且最好在同一子网中。这有助于对检测信号的源/目标对进行配对。如果在要添加的成员与群集中的现有成员之间检测不到公共子网 (使用 IP 地址/子网掩码)，将生成此事件并且配置任务会失败。事件详细信息会报告现有成员上没有的加入成员的子网。 \
       </description>\
       <cause>\
            <description>\
                主机拥有现有群集成员上缺少的多余网络\
            </description>\
            <action>\
                更改主机的网络配置，以便在群集中的现有主机所在的子网中启用 vSphere HA 流量。在 ESX 上，vSphere HA 将使用“服务控制台”端口组；在 ESXi 主机上，vSphere HA 将使用选中了“管理流量”复选框的端口组。\
            </action>\
            <action>\
                使用高级选项替代选择用于 vSphere HA 群集通信的默认端口组。可以使用 das.allowNetwork[X] 高级选项告知 vSphere HA 使用此选项中指定的端口组。对于每个应该使用的端口组名称，指定一个 das.allowNetwork[X] 高级选项。vSphere HA 配置会检查要为与指定名称匹配的端口组添加的主机。配置任务还会检查其端口组与指定名称匹配的现有成员。每个主机上匹配的端口组名称数必须相同。设置高级选项后，请重新启用群集的 vSphere HA。\
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostNoHAEnabledPortGroupsEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostNoHAEnabledPortGroupsEvent">\
       <description>\
       vSphere HA 已确定主机上没有可用于 \
       vSphere HA 代理间通信的管理网络。\
       </description>\
       <cause>\
            <description>\
                该主机没有可用的 vSphere HA 管理网络\
            </description>\
            <action>\
                如果在把主机添加到 vSphere HA 群集时出现此事件， \
                请更改主机的网络配置，以便在一个或多个端口组上 \
                启用 vSphere HA 流量。 默认情况下，vSphere HA  将 \
                在 ESX 和 ESXi 主机上使用“服务控制台”端口组，也就是 \
                选中了“管理流量”复选框的端口组。如果主机上已配置 \
                vSphere HA，则可能是主机的网络设置已更改，管理网络 \
                配置已失效。请查看设置，确保主机上仍存在为管理网络 \
                配置的端口组，并且已启用 ESXi 的“管理流量”选项。修复 \
                所有配置问题后，请在主机上重新配置 vSphere HA。\
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostNoAvailableNetworksEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostNoAvailableNetworksEvent">\
       <description>\
       要添加到 vSphere HA 群集的主机没有可用于 vSphere HA 群集通信的管理网络。设置了高级选项 das.allowNetwork[X]，但没有与此主机的高级选项匹配的端口组名称。\
       </description>\
       <cause>\
            <description>\
                该主机没有与高级选项用来控制 vSphere HA 使用的端口组的名称匹配的端口组\
            </description>\
            <action>\
                删除高级选项 das.allowNetwork[X]，以便 vSphere HA 选择默认管理端口组\
            </action>\
            <action>\
                更正高级选项中指定的端口组的名称，使之与 vSphere HA 用于此主机的名称匹配\
            </action>\
            <action>\
                指定其他 das.allowNetwork[X] 高级选项，以匹配此主机的端口组名称\
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostIsolationIpPingFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostIsolationIpPingFailedEvent">\
       <description>\
        vSphere HA 无法 Ping 一个或多个隔离 IP 地址。无法 Ping 地址可能会 \
        导致 HA 错误地声明主机处于网络隔离状态。如果主机无法 Ping 配置 \
        的隔离地址并且此主机上的 vSphere HA 代理无法访问其他群集主机上 \
        运行的任何代理，则该主机会被声明处于隔离状态。 \
       </description>\
       <cause>\
            <description>无法 ping 隔离地址</description>\
            <action>更正 ping 该地址失败的原因</action>\
            <action>\
                使用高级选项更改 vSphere HA 用于确定主机是否处于网络隔离状态 \
                的地址。默认情况下，隔离地址是管理网络的默认 \
        	网关。您可以使用高级选项替代默认值，或者指定 \
        	使用其他地址来确定主机是否处于网络隔离状态。  \
        	如果不希望 vSphere HA 将默认网关用作隔离地址，请将 \
		das.useDefaultIsolationAddress 高级选项设置为“false”。 \
		为要指定的每个隔离地址指定 das.isolationAddress[X] 高级 \
        	选项。为每个主机重新配置 vSphere HA 时，新值将生效。\
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostNoRedundantManagementNetworkEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostNoRedundantManagementNetworkEvent">\
       <description>\
       vSphere HA 已确定，只有一条路径供 vSphere HA 管理流量使用，导致单点故障。最佳实践要求，应有多条路径供 vSphere HA 用于检测信号和群集通信。出现网络故障后，具有单一路径的主机更可能被声明为已停止运行、网络已分区或已隔离。如果被声明为已停止运行，主机随后实际出现故障时 vSphere HA 将不会响应；如果被声明为已隔离，vSphere HA 可能应用隔离响应，从而影响虚拟机在主机上运行的正常运行时间。\
       </description>\
       <cause>\
            <description>仅有一个端口组可用于 vSphere HA 通信</description>\
            <action>在 ESX 主机上配置其他服务控制台端口组</action>\
            <action>\
                通过选中“管理流量”复选框，在 ESXi 主机上配置其他端口组\
            </action>\
            <action>\
                对管理端口组使用网卡绑定，以便在路径出现故障时，允许 ESX 或 ESXi 将管理流量定向到多个物理网卡外部\
            </action>\
            <action>\
                如果接受未对 vSphere HA 通信提供冗余的风险，则可以通过将 das.ignoreRedundantNetWarning 高级选项设置为“true”来消除配置问题\
            </action>\
       </cause>\
    </EventLongDescription>\
    "

HostIpToShortNameFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostIpToShortNameFailedEvent">\
       <description>\
          无法将主机的 IP 地址解析为短名称 \
       </description>\
       <cause>\
            <description>未正确配置主机或 DNS 记录</description>\
            <action>检查主机网络配置</action>\
            <action>检查 DNS 配置</action>\
        </cause>\
    </EventLongDescription>\
    "

HostGetShortNameFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostGetShortNameFailedEvent">\
       <description>\
          在主机上执行 hostname -s 命令失败 \
       </description>\
       <cause>\
            <description>未正确配置主机网络</description>\
            <action>检查主机网络配置</action>\
        </cause>\
    </EventLongDescription>\
    "

HostPrimaryAgentNotShortNameEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostPrimaryAgentNotShortNameEvent">\
       <description>\
          未以短名称格式指定主代理 \
       </description>\
       <cause>\
            <description>未正确配置主机网络</description>\
            <action>检查主机网络配置</action>\
        </cause>\
    </EventLongDescription>\
    "

HostShortNameInconsistentEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostShortNameInconsistentEvent">\
       <description>\
          对主机的名称检查返回不同的主机名称 \
       </description>\
       <cause>\
            <description>未正确配置主机网络</description>\
            <action>检查主机网络配置</action>\
            <action>检查 DNS 配置</action>\
        </cause>\
    </EventLongDescription>\
    "

HostShortNameToIpFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostShortNameToIpFailedEvent">\
       <description>\
          无法将主机的短名称解析为 IP 地址 \
       </description>\
       <cause>\
            <description>未正确配置主机网络</description>\
            <action>检查主机网络配置</action>\
            <action>检查 DNS 配置</action>\
        </cause>\
    </EventLongDescription>\
    "
