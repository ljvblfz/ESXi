VmFailedToShutdownGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToShutdownGuestEvent">\
      <description>\
         Échec d'arrêt du système d'exploitation client pour la machine virtuelle\
      </description>\
      <cause>\
         <description>\
            L'arrêt du système d'exploitation client peut échouer si VMware Tools  n'est pas installé dans la machine virtuelle.\
         </description>\
         <action>Installez VMware Tools.</action>\
      </cause>\
      <cause>\
         <description>\
	    Il se peut que la machine virtuelle attende une réponse à une question ou une invite</description>\
         <action>\
            Allez sur l'onglet Résumé de la machine virtuelle dans vSphere Client et répondez à la question  ou à l'invite\
         </action>\
      </cause>\
      <cause>\
         <description>\
            D'autres systèmes peuvent fonctionner simultanément sur la machine virtuelle\
         </description>\
         <action>Mettez fin aux opérations simultanées et retentez l'opération d'arrêt</action>\
      </cause>\
      <cause>\
         <description>La machine virtuelle est dans un état incorrect. Les machines virtuelles peuvent entrer dans  un état incorrect pour de nombreuses raisons, par exemple l'inaccessibilité de la banque de données.</description>\
         <action>\
           Identifiez la raison pour laquelle la machine virtuelle est entrée dans un état incorrect, corrigez le problème, et retentez l'opération.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

AlarmEmailFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.AlarmEmailFailedEvent">\
       <description>\
          Une erreur s'est produite pendant l'envoi de la notification e-mail lors d'un déclenchement d'alarme\
       </description>\
       <cause>\
            <description>N'a pas envoyé l'e-mail pour une alarme déclenchée</description>\
            <action>Vérifiez les paramètres SMTP de vCenter Server pour l'envoi de notifications par e-mail</action>\
       </cause>\
    </EventLongDescription>\
    "
    
AlarmReconfiguredEvent.longDescription = "\
    <EventLongDescription id="vim.event.AlarmReconfiguredEvent">\
       <description>\
          Une alarme a été reconfigurée \
       </description>\
       <cause>\
            <description>Un utilisateur a reconfiguré une alarme</description>\
       </cause>\
    </EventLongDescription>\
    "
        
AlarmScriptFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.AlarmScriptFailedEvent">\
       <description>\
          vCenter Server enregistre cet événement si une erreur se produit lors de l'exécution d'un script après le déclenchement d'une alarme.\
       </description>\
       <cause>\
            <description>Une erreur s'est produite lors de l'exécution du script</description>\
            <action>Corrigez le script ou l'état d'échec</action>\
       </cause>\
    </EventLongDescription>\
    "

    
AlarmSnmpFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.AlarmSnmpFailedEvent">\
       <description>\
          vCenter Server enregistre cet événement si une erreur se produit lors de l'envoie d'un piège SNMP au déclenchement d'une alarme.\
       </description>\
       <cause>\
            <description>Impossible d'envoyer un piège SNMP pour une alarme déclenchée</description>\
            <action>Vérifiez les paramètres SNMP de vCenter Server. Vérifiez que le réseau de vCenter Server peut gérer des paquets SNMP.</action>\
       </cause>\
    </EventLongDescription>\
    "
   
   
ScheduledTaskEmailFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.ScheduledTaskEmailFailedEvent">\
       <description>\
          Une erreur s'est produite pendant l'envoi de la notification d'e-mail indiquant qu'une tâche programmée est exécutée\
       </description>\
       <cause>\
            <description>Échec d'envoi de l'e-mail pour la tâche planifiée</description>\
            <action>Vérifiez les paramètres SMTP de vCenter Server pour l'envoi d'e-mails</action>\
       </cause>\
    </EventLongDescription>\
    "

ScheduledTaskFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.ScheduledTaskFailedEvent">\
       <description>\
          Une erreur s'est produite lors de l'exécution d'une tâche planifiée \
       </description>\
       <cause>\
            <description>Échec d'exécution d'une tâche planifiée</description>\
            <action>Corrigez l'état d'échec</action>\
       </cause>\
    </EventLongDescription>\
    "


DasAgentUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.DasAgentUnavailableEvent">\
       <description>\
         vCenter Server ne peut contacter aucun bon hôte primaire dans le cluster vSphere HA.  Il se peut que la protection vSphere HA ne soit pas disponible pour des machines virtuelles en cours d'exécution dans le cluster.  En outre, vous ne pouvez pas activer ou reconfigurer vSphere HA sur les hôtes dans  le cluster tant que le contact entre vCenter Server et un bon hôte primaire n'est pas rétabli.\
       </description>\
       <cause>\
          <description>\
             En raison d'une panne de réseau, tous les hôtes apparaissent dans l'inventaire comme « ne répondant pas »\
          </description>\
          <action>Restaurez le réseau</action>\
       </cause>\
       <cause>\
          <description>Échec de tous les hôtes primaires dans le cluster</description>\
          <action>\
            Si les hôtes primaires défaillants ne peuvent pas être restaurés, désactivez vSphere HA sur le cluster,  attendez la fin des tâches d'annulation de configuration de vSphere HA sur tous les hôtes, puis  réactivez vSphere HA sur le cluster\
          </action>\
       </cause>\
    </EventLongDescription>\
    "

DasDisabledEvent.longDescription = "\
    <EventLongDescription id="vim.event.DasDisabledEvent">\
       <description>\
          vSphere HA a été désactivé sur cet hôte en raison d'une action de l'utilisateur. vSphere HA est désactivé quand un hôte  est déconnecté de vCenter Server ou placé en mode maintenance ou standby.  Les machines virtuelles sur les autres hôtes dans le cluster n'échoueront pas  sur cet hôte en cas d'échec de l'hôte. En outre, si l'hôte est déconnecté,  toutes les machines virtuelles en cours d'exécution sur cet hôte n'échoueront pas en cas d'échec de l'hôte. De plus,  aucune tentative ne sera faite par la VM vSphere HA et le contrôle d'application pour réinitialiser les VM. \
       </description>\
    </EventLongDescription>\
    "

DasEnabledEvent.longDescription = "\
    <EventLongDescription id="vim.event.DasEnabledEvent">\
       <description>\
         vSphere HA a été activé sur cet hôte à la suite d'une action de l'utilisateur. vSphere HA est activé \
         quand un hôte est ajouté à ou déplacé vers un cluster vSphere HA ou quand vSphere HA est activé sur \
         un cluster. Si l'hôte était déjà dans un cluster vSphere HA, vSphere HA est activé quand l'hôte est reconnecté à vCenter \
         Server ou sorti du mode de maintenance ou veille. vSphere HA essayera de protéger toutes les VM qui sont en cours \
         d'exécution sur l'hôte au moment où HA est activé sur celui-ci. \
       </description>\
    </EventLongDescription>\
    "

HostDasEnablingEvent.longDescription = "\
     <EventLongDescription id="vim.event.HostDasEnablingEvent">\
        <description>\
           vSphere HA est en cours d'activation sur cet hôte.\
        </description>\
     </EventLongDescription>\
     "

InsufficientFailoverResourcesEvent.longDescription = "\
     <EventLongDescription id="vim.event.InsufficientFailoverResourcesEvent">\
        <description>\
           Le cluster n'a pas de capacité non réservée suffisante pour satisfaire le niveau configuré  pour le contrôle d'admission vSphere HA. Il reste possible que vSphere HA effectue des basculements mais ce sera  dans le cadre d'un service au mieux.\
        </description>\
        <cause>\
            <description>\
               Si la règle de « nombre de défaillances d'hôte à tolérer » est configurée et que quelques  machines virtuelles ont une réservation de CPU ou de mémoire supérieurs à celles des autres  machines virtuelles, le contrôle d'admission vSphere HA peut être trop conservateur  pour s'assurer qu'il y a assez de ressources défragmentées en cas d'échec d'un hôte.\
            </description>\
            <action>\
                Utilisez un CPU identique et des réservations de mémoire pour toutes les machines virtuelles  dans le cluster. Sinon, envisagez l'utilisation d'une règle de contrôle d'admission vSphere HA différente  telle que la réservation d'un pourcentage de la ressource de clusters en cas de basculement.  Vous pouvez également utiliser les options avancées pour spécifier un seuil pour la taille du logement.  Pour plus d'informations, consultez le Guide de disponibilité vSphere.\
            </action>\
        </cause>\
        <cause>\
           <description>\
                Les hôtes présentant des erreurs d'agent vSphere HA ne sont pas de bons candidats  pour offrir une capacité de basculement dans le cluster et leurs ressources ne sont  pas prises en compte à des fins de contrôle d'admission vSphere HA. Si de nombreux hôtes présentent une  erreur d'agent vSphere HA, vCenter Server génère cet événement.\
           </description>\
           <action>\
              Consultez le journal d'événements des hôtes pour déterminer la cause des erreurs d'agent vSphere HA.  Après avoir abordé tous les problèmes de configuration, reconfigurez vSphere HA sur les hôtes  concernés ou sur le cluster.\
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmDasBeingResetEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmDasBeingResetEvent">\
        <description>\
          La machine virtuelle a été réinitialisée par vSphere HA. Selon la façon dont vSphere HA a été configuré,  il se peut que la machine virtuelle soit réinitialisée car l'état du battement de cœur VMware Tools ou  de l'application est devenu rouge.\
        </description>\
        <cause>\
           <description>\
               Le battement de cœur de VMware Tools est devenu rouge. Cela peut se produire si le système d'exploitation  est entré en échec avec un écran bleu ou cesse de répondre. Cela peut également se produire en cas  d'échec ou d'arrêt de VMware Tools.\
           </description>\
           <action>\
               Si la machine virtuelle est réinitialisée fréquemment, recherchez  un problème persistant dans le système d'exploitation qui nécessiterait votre attention.  Envisagez de configurer le cluster de manière que vSphere HA attende plus longtemps  après la perte des battements de cœur avant d'agir. Cela permet d'éviter  de déclencher des réinitialisations pour des problèmes passagers. Vous pouvez définir une période plus longue en diminuant  la « sensibilité de surveillance » dans la section de surveillance de VM de l'Assistant de modification de cluster.\
           </action>\
        </cause>\
        <cause>\
           <description>\
               Le battement de cœur de l'application est devenu rouge. Cet état peut se produire si l'application  qui est configurée pour envoyer des battements de cœur est défaillante ou a cessé de répondre.\
           </description>\
           <action>\
               Déterminez si l'application a cessé d'envoyer des battements de cœur en raison d'une erreur  de configuration et résolvez le problème.\
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmDasBeingResetWithScreenshotEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmDasBeingResetWithScreenshotEvent">\
        <description>\
          La machine virtuelle a été réinitialisée par vSphere HA. Selon la façon dont vSphere HA est configuré, cet état peut se produire parce que l'état de battement de cœur de VMware Tools ou  de l'application est devenu rouge. L'événement contient l'emplacement de la capture d'écran prise de la console invitée avant sa réinitialisation. Vous pouvez utiliser ces informations pour déterminer la cause de la défaillance des battements de cœur.\
        </description>\
        <cause>\
           <description>\
               Le battement de cœur de VMware Tools est devenu rouge. Cela peut se produire si le système d'exploitation  est entré en échec avec un écran bleu ou cesse de répondre. Cela peut également se produire en cas  d'échec ou d'arrêt de VMware Tools.\
           </description>\
           <action>\
                Vérifiez l'image de capture d'écran pour voir si la cause était une défaillance du système d'exploitation client. Si la machine virtuelle est réinitialisée fréquemment, cherchez  un problème persistant dans le système d'exploitation qui nécessiterait votre attention.  Envisagez de configurer le cluster de manière que vSphere HA attende plus longtemps  après la perte des battements de cœur avant d'agir. Cela permet d'éviter  de déclencher des réinitialisations pour des problèmes passagers. Vous pouvez définir une période plus longue en diminuant  la « sensibilité de surveillance » dans la section de surveillance de VM de l'Assistant de modification de cluster.\
           </action>\
        </cause>\
        <cause>\
           <description>\
               Le battement de cœur de l'application est devenu rouge. Cet état peut se produire si l'application  qui est configurée pour envoyer des battements de cœur est défaillante ou a cessé de répondre.\
           </description>\
           <action>\
               Déterminez si l'application a cessé d'envoyer des battements de cœur en raison d'une erreur  de configuration et résolvez le problème.\
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmDasResetFailedEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmDasResetFailedEvent">\
        <description>\
           vSphere HA a tenté de réinitialiser la machine virtuelle en raison d'une défaillance des battements de cœur  de VMware Tools ou d'une application invitée, selon la façon dont vSphere HA a été configuré. Cependant, l'opération de réinitialisation a échoué.\
        </description>\
        <cause>\
            <description>\
                La raison la plus probable de l'échec de réinitialisation est que la machine virtuelle exécutait  une autre tâche au moment de la réinitialisation.\
            </description>\
            <action>Vérifiez si la machine virtuelle nécessite votre attention et réinitialisez-la  manuellement au besoin.</action>\
        </cause>\
     </EventLongDescription>\
     "

VmFailoverFailed.longDescription = "\
     <EventLongDescription id="vim.event.VmFailoverFailed">\
        <description>\
           vSphere HA n'a pas basculé cette machine virtuelle. L'événement inclut les informations sur l'échec qui \
           a été généré quand vSphere HA a essayé de basculer. vSphere HA réessayera le basculement sur un autre \
           hôte, sauf si le nombre maximal de tentatives de basculement a été dépassé. Dans de nombreux cas, \
           la nouvelle tentative réussira.\
        </description>\
        <cause>\
           <description>\
                Le basculement a échoué, car un problème est survenu pendant que vSphere HA essayait \
                de redémarrer  la machine virtuelle. Les problèmes potentiels incluent l'incapacité à enregistrer \
                ou reconfigurer la machine virtuelle sur le nouvel hôte étant donné qu'une autre\
                opération sur la même machine virtuelle est déjà en cours ou que la machine virtuelle est \
                toujours sous tension. Cela peut également se produire si le fichier de configuration de la machine virtuelle est corrompu.\
           </description>\
           <action> \
                   Si vSphere HA est incapable de basculer la machine virtuelle après des tentatives répétées, rechercher l'erreur signalée par chaque occurrence de cet événement ou essayer de mettre sous tension la machine virtuelle et rechercher toutes les erreurs retournées.\
           </action> \
           <action> \
                   Si l'erreur signale qu'un fichier est verrouillée, la VM peut mettre sous tension un hôte que l'agent maître vSphere HA ne peut plus contrôler en utilisant le réseau de gestion ou les banques de données de signal de pulsation, ou elle peut avoir été mise sous tension par un utilisateur sur un hôte en dehors du cluster. Si tous les hôtes ont été déclarés morts, rechercher si un problème de mise en réseau stockage peut en être la cause. \
           </action> \
           <action> \
                   Cependant, si l'erreur signale que la machine virtuelle est dans un état non valide, il est possible qu'une opération en cours empêche l'accès aux fichiers de la machine virtuelle. Recherchez si des opérations sont en cours, notamment une opération de clonage qui prend beaucoup de temps. \
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmMaxRestartCountReached.longDescription = "\
     <EventLongDescription id="vim.event.VmMaxRestartCountReached">\
        <description>\
           vSphere HA a atteint le nombre maximal de tentatives de basculement pour cette machine \
           virtuelle et n'a pas été capable de la redémarrer. Aucune autre tentative de \
           basculement ne sera effectuée. Par défaut, vSphere HA tente de basculer une machine virtuelle 5 fois. \
        </description>\
        <cause>\
           <description>\
                Le basculement peut échouer pour plusieurs raisons :  \
                le fichier de configuration de la VM est corrompu ou plusieurs banques de données des \
                machines virtuelles ne sont pas accessibles par un hôte du cluster en raison d'une\
                condition défaillance de tous les chemins. De plus, la VM peut être mise sous tension sur un hôte que \
                l'agent principal vSphere HA ne peut plus contrôler en utilisant le réseau de gestion ou les banques de données \
                des signaux de pulsations, ou elle peut avoir été mise sous tension par un utilisateur d'un hôte extérieur au cluster.\
                </description>\
           <action>\
                Pour déterminer les raisons pour lesquelles les tentatives de basculement précédentes \
                ont échoué, recherchez dans les événements de la machine virtuelle consignés les occurrences \
                d'événements signalées par vSphere HA lors d'un échec de basculement. Ils indiquent le motif de l'échec \
                du basculement. Pour rechercher des événements vSphere HA, il suffit d'entrer la chaîne\
                de recherche « vSphere HA ». Pour déterminer s'il existe des problèmes, essayez de \
                mettre manuellement sous tension la machine virtuelle. Si la mise sous tension échoue, \
                recherchez l'erreur qui est renvoyée. En revanche, si la mise sous tension reste en attente pendant \
                un long moment, recherchez s'il existe une condition de défaillance pour tous les chemins. En outre, si un hôte a été déclaré inactif, \
                recherchez si un problème de mise en réseau ou de stockage peut en être la cause. \
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmPowerOffOnIsolationEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmPowerOffOnIsolationEvent">\
        <description>\
          vSphere HA a mis cette machine virtuelle hors tension parce que l'hôte sur lequel elle s'exécutait  était isolé du réseau de gestion. \
        </description>\
     </EventLongDescription>\
     "

VmShutdownOnIsolationEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmShutdownOnIsolationEvent">\
        <description>\
          vSphere HA a arrêté cette machine virtuelle parce que l'hôte sur lequel elle s'exécutait  était isolé du réseau de gestion.\
        </description>\
     </EventLongDescription>\
     "

VmAutoRenameEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmAutoRenameEvent">\
       <description>\
          La machine virtuelle a été renommée en raison de conflits de nom potentiels avec une autre machine virtuelle\
       </description>\
       <cause>\
            <description>Il se peut que la machine virtuelle ait été ajoutée à l'inventaire de vCenter Server pendant que le balayage des banques de données des hôtes ajoutait à l'inventaire. Pendant ce type d'action, le nom de la machine virtuelle nouvellement ajoutée peut se trouver en conflit avec un nom de machine virtuelle déjà dans l'inventaire. Pour résoudre ce problème, vCenter Server renomme la machine virtuellement nouvellement ajoutée.\
            </description>\
        </cause>\
    </EventLongDescription>\
    "

VmBeingDeployedEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmBeingDeployedEvent">\
        <description>\
           Une machine virtuelle est en cours de création à partir d'un modèle\
        </description>\
        <cause>\
             <description>\
                Une action de l'utilisateur a demandé la création d'une machine virtuelle à partir de ce modèle.\
             </description>\
         </cause>\
     </EventLongDescription>\
     "

VmBeingHotMigratedEvent.longDescription = "\
  <EventLongDescription id="vim.event.VmBeingHotMigratedEvent">\
     <description>\
        Une machine virtuelle sous tension est en cours de migration avec vMotion\
     </description>\
     <cause>\
          <description> Une action de l'utilisateur peut avoir entraîné la migration d'une machine virtuelle sous tension avec vMotion\
          </description>\
      </cause>\
       <cause>\
          <description> Une recommandation DRS peut avoir entraîné la migration d'une machine virtuelle sous tension avec vMotion\
          </description>\
      </cause>\
  </EventLongDescription>\
  "

DatastoreDiscoveredEvent.longDescription = "\
  <EventLongDescription id="vim.event.DatastoreDiscoveredEvent">\
     <description>\
        Une banque de données a été détectée sur un hôte\
     </description>\
     <cause>\
        <description>\
           Un hôte ayant accès à cette banque de données a été ajouté au centre de données\
        </description>\
     </cause>\
     <cause>\
        <description>\
           Le stockage sauvegardant cette banque de données a été démasqué à un hôte dans le centre de données\
        </description>\
     </cause>\
     <cause>\
        <description>\
           Une action de l'utilisateur ou du système a entraîné la création de cette banque de données sur un hôte\
        </description>\
     </cause>\
     <cause>\
        <description>\
          Une action de l'utilisateur ou du système a entraîné la création de cette banque de données sur un hôte et la banque de données était visible sur au moins un autre hôte dans le centre de données avant cette opération. \
        </description>\
     </cause>\
   </EventLongDescription>\
   "

DatastoreRenamedOnHostEvent.longDescription = "\
   <EventLongDescription id="vim.event.DatastoreRenamedOnHostEvent">\
      <description>\
         Une banque de données a été renommée sur un hôte géré par vCenter Server\
      </description>\
      <cause>\
         <description>\
           vCenter Server a détecté la banque de données sur un hôte et renommé la banque de données parce qu'elle existe déjà dans l'inventaire vCenter Server sous un nom différent. vCenter Server peut également avoir renommé la banque de données parce que le nom est en conflit avec une autre banque de données dans le même centre de données. \
         </description>\
      </cause>\
   </EventLongDescription>\
  "

VMFSDatastoreExtendedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VMFSDatastoreExtendedEvent">\
      <description>\
         Un volume VMFS existant a été étendu pour augmenter sa capacité\
      </description>\
      <cause>\
         <description>\
           Une action de l'utilisateur ou du système a entraîné l'extension de la banque de données avec une partition sur un LUN pour augmenter sa capacité. \
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VMFSDatastoreExpandedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VMFSDatastoreExpandedEvent">\
      <description>\
         Une étendue existante dans un volume VMFS a été accrue pour augmenter sa capacité\
      </description>\
      <cause>\
         <description>\
           Une action de l'utilisateur ou du système a entraîné l'extension d'une banque de données   VMFS existante. Seules les étendues dotées d'espace libre immédiatement après elles  sont extensibles. En conséquence, l'action a rempli la capacité adjacente disponible sur le LUN.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmConfigMissingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmConfigMissingEvent">\
      <description>\
         Un ou plusieurs fichiers de configuration pour la machine virtuelle sont introuvables\
      </description>\
      <cause>\
         <description>\
            La banque de données sur laquelle cette machine virtuelle réside peut être inaccessible\
         </description>\
         <action>\
            Vérifiez la connectivité de la banque de données sur laquelle cette machine virtuelle  réside. Si la banque de données a un LUN de sauvegarde, vérifiez si des défaillances de disque passagères sont présentes.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmDeployedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmDeployedEvent">\
      <description>\
         Une machine virtuelle a été créée à partir du modèle spécifié\
      </description>\
      <cause>\
         <description>\
            Une action de l'utilisateur a entraîné la création d'une machine virtuelle à partir du modèle\
         </description>\
      </cause>\
      <cause>\
         <description>\
            Une tâche planifiée a entraîné la création d'une machine virtuelle à partir du modèle\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmMacChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmMacChangedEvent">\
      <description>\
         L'adresse MAC de la machine virtuelle a changé\
      </description>\
      <cause>\
         <description>\
            Une action de l'utilisateur a changé l'adresse MAC de la machine virtuelle\
         </description>\
      </cause>\
      <cause>\
         <description>\
           vCenter a changé l'adresse MAC de la machine virtuelle parce qu'il a détecté un conflit d'adresses MAC\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmMacConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmMacConflictEvent">\
      <description>\
        L'adresse MAC de la machine virtuelle est en conflit avec celle d'une autre machine virtuelle\
      </description>\
      <cause>\
         <description> L'adresse MAC de cette machine virtuelle est identique à celle d'une autre machine virtuelle. Reportez-vous aux détails de l'événement pour plus d'informations sur la machine virtuelle qui a provoqué le conflit.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmOrphanedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmOrphanedEvent">\
      <description>\
         La machine virtuelle n'existe pas sur l'hôte auquel elle est associée\
      </description>\
      <cause>\
         <description> La machine virtuelle a été supprimée pendant que son hôte était déconnecté de vCenter Server. \
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmConnectedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmConnectedEvent">\
      <description>\
        La machine virtuelle est dans un état connecté dans l'inventaire  et vCenter Server peut y accéder\
      </description>\
      <cause>\
         <description>\
           Une action de l'utilisateur ou du système qui a entraîné des opérations telles que la création, l'enregistrement, le clonage ou le déploiement d'une machine virtuelle a donné à vCenter Server l'accès à  la machine virtuelle\
         </description>\
      </cause>\
       <cause>\
         <description>\
           Une action de l'utilisateur ou du système qui a entraîné des opérations telles que l'ajout ou  la reconnexion d'un hôte a donné à vCenter Server l'accès à la machine virtuelle\
         </description>\
      </cause>\
      <cause>\
         <description>\
           L'état de l'hôte de machine virtuelle a changé de Not Responding en Connected et l'hôte a donné à vCenter Server l'accès à la machine virtuelle\
         </description>\
      </cause>\
    </EventLongDescription>\
    "
   MigrationErrorEvent.longDescription = "\
         <EventLongDescription id="vim.event.MigrationErrorEvent">\
            <description>\
               Échec de migration d'une machine virtuelle car elle n'a pas rempli tous les critères de compatibilité\
            </description>\
            <cause>\
                 <description> La migration d'une machine virtuelle de l'hôte source a échoué, car  la machine virtuelle n'a pas rempli tous les critères de compatibilité\
                 </description>\
                  <action> \
                      Utilisez vSphere Client pour rechercher des erreurs au moment de l'échec pour pouvoir identifier les raisons possibles de ce problème\
                  </action>\
             </cause>\
         </EventLongDescription>\
         "

 MigrationHostErrorEvent.longDescription = "\
         <EventLongDescription id="vim.event.MigrationHostErrorEvent">\
            <description>\
               Échec de migration d'une machine virtuelle, car elle n'a pas rempli tous les critères de compatibilité\
            </description>\
            <cause>\
                 <description> La migration d'une machine virtuelle vers l'hôte ou la banque de données de destination a échoué, car la machine virtuelle n'a pas rempli tous les critères de compatibilité\
                 </description>\
                  <action> \
                      Utilisez vSphere Client pour rechercher des erreurs au moment de l'échec pour pouvoir identifier les raisons possibles de ce problème\
                  </action>\
             </cause>\
         </EventLongDescription>\
         "

VmBeingMigratedEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmBeingMigratedEvent">\
        <description>\
           Changement de l'hôte sur lequel la machine virtuelle est en cours d'exécution\
        </description>\
        <cause>\
           <description>\
                Une action de l'utilisateur a entraîné la migration de la machine virtuelle vers un hôte différent\
           </description>\
        </cause>\
     </EventLongDescription>\
     "

VmMigratedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmMigratedEvent">\
      <description>\
         L'hôte de la machine virtuelle a été changé\
      </description>\
      <cause>\
         <description>\
Une action de l'utilisateur a entraîné la migration  de la machine virtuelle vers un hôte différent\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmBeingRelocatedEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmBeingRelocatedEvent">\
        <description>\
           L'exécution et/ou le stockage de la machine virtuelle est déplacé(e)\
        </description>\
        <cause>\
           <description>\
          Une action de l'utilisateur peut avoir entraîné la modification de l'exécution et/ou du stockage de la machine virtuelle\
           </description>\
        </cause>\
     </EventLongDescription>\
     "

VmRelocatedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmRelocatedEvent">\
      <description>\
         L'exécution et/ou le stockage de la machine virtuelle a été déplacé(e)\
      </description>\
      <cause>\
         <description>\
               Une action de l'utilisateur a entraîné la modification de l'exécution et/ou du stockage de la machine virtuelle\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmCloneFailedEvent.longDescription = "\
     <EventLongDescription id="vim.event.VmCloneFailedEvent">\
        <description>\
           Échec de clonage d'une machine virtuelle\
        </description>\
        <cause>\
           <description>\
                Une erreur a empêché le clonage de la machine virtuelle\
           </description>\
        </cause>\
     </EventLongDescription>\
     "

MigrationHostWarningEvent.longDescription = "\
         <EventLongDescription id="vim.event.MigrationHostWarningEvent">\
            <description>\
              La machine virtuelle peut être migrée mais pourrait perdre certaines fonctionnalités après la migration\
            </description>\
            <cause>\
                 <description> La migration de la machine vers l'hôte ou la banque de données de destination est susceptible d'aboutir mais certaines fonctionnalités pourraient ne pas fonctionner correctement après, car la machine virtuelle n'a pas rempli tous les critères de compatibilité.\
                 </description>\
                  <action> \
                      Utilisez vSphere Client pour rechercher des avertissements au moment de l'échec pour pouvoir identifier les raisons possibles de ce problème\
                  </action>\
             </cause>\
         </EventLongDescription>\
         "

MigrationResourceErrorEvent.longDescription = "\
         <EventLongDescription id="vim.event.MigrationResourceErrorEvent">\
            <description>\
               Échec de migration d'une machine virtuelle en raison d'incompatibilités avec le pool de ressources cible\
            </description>\
            <cause>\
                 <description>La migration d'une machine virtuelle vers l'hôte ou vers la banque de données de destination est impossible en raison d'incompatibilités avec le pool de ressources cible.\
                 </description>\
                  <action> \
                      Utilisez vSphere Client pour rechercher des erreurs au moment de l'échec pour pouvoir identifier les raisons possibles de ce problème\
                  </action>\
             </cause>\
         </EventLongDescription>\
         "

MigrationResourceWarningEvent.longDescription = "\
         <EventLongDescription id="vim.event.MigrationResourceWarningEvent">\
            <description>\
              La machine virtuelle peut être migrée mais pourrait perdre certaines fonctionnalités  après la migration\
            </description>\
            <cause>\
                 <description> La migration de la machine vers le pool de ressources de destination est susceptible d'aboutir mais certaines fonctionnalités pourraient ne pas fonctionner correctement après, car la machine virtuelle n'a pas rempli tous les critères de compatibilité.\
                 </description>\
                  <action> \
                      Utilisez vSphere Client pour rechercher des avertissements au moment de l'échec pour pouvoir identifier les raisons possibles de ce problème\
                  </action>\
             </cause>\
         </EventLongDescription>\
         "

VmDeployFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmDeployFailedEvent">\
        <description>\
           Échec de déploiement d'une machine virtuelle pour des raisons décrites dans le message d'événement\
        </description>\
        <cause>\
           <description>\
               Échec de déploiement de la machine virtuelle.  Cette condition peut se produire si l'espace disque est insuffisant, si l'hôte ou la machine virtuelle perd  sa connexion au réseau, si l'hôte est déconnecté, etc.\
           </description>\
          <action>\
	   Identifiez la raison dans le message d'événement pour déterminer la cause de l'échec et corriger le problème.\
         </action>\
	</cause>\
     </EventLongDescription>\
     "

VmDiskFailedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmDiskFailedEvent">\
        <description>\
           Échec de création d'un disque virtuel pour la machine virtuelle pour des raisons décrites dans le message d'événement\
        </description>\
        <cause>\
           <description>\
                Un disque virtuel n'a pas été créé pour la machine virtuelle. Cette condition peut se produire si  l'opération n'a pas réussi à accéder au disque, le disque n'a pas eu assez d'espace, vous n'avez pas  l'autorisation pour l'opération et ainsi de suite.\
           </description>\
           <action>\
                Vérifiez la raison dans le message d'événement pour trouver la cause de l'échec. Assurez-vous que le disque est accessible, qu'il dispose de suffisamment d'espace, et que les paramètres d'autorisation permettent l'opération.\
           </action>\
        </cause>\
     </EventLongDescription>\
     "

VmFailedMigrateEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedMigrateEvent">\
        <description>\
           Échec de migration de la machine virtuelle pour des raisons décrites dans le message d'événement\
        </description>\
        <cause> \
            <description> \
               La machine virtuelle n'a pas été migrée. Cet état peut se produire si les adresses IP vMotion ne sont pas configurées,  si les hôtes source et de destination ne sont pas accessibles, etc.\
            </description> \
            <action> \
                Vérifier le motif dans le message d'événement pour trouver la cause de l'échec. S'assurer que les IP  vMotion sont configurés sur les hôtes source et de destination, que les hôtes sont accessibles, etc.\
            </action> \
        </cause> \
     </EventLongDescription>\
     "

VmFailedRelayoutOnVmfs2DatastoreEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedRelayoutOnVmfs2DatastoreEvent">\
        <description>\
           Échec de migration d'une machine virtuelle sur la banque de données VMFS2\
        </description>\
        <cause>\
           <description>\
                Une tentative de migrer une machine virtuelle a échoué car  la machine virtuelle a toujours un (des) disque(s) sur une banque de données VMFS2. Les banques de données VMFS2 sont en lecture seule pour  les hôtes ESX 3.0 et ultérieurs.\
           </description>\
           <action> \
                Mettez à jour les banques de données de VMFS2 à VMFS3\
           </action> \
        </cause>\
     </EventLongDescription>\
     "

VmFailedRelayoutEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedRelayoutEvent">\
        <description>\
           Échec d'agencement de la machine virtuelle \
        </description>\
        <cause>\
           <description>\
               Une tentative d'agencement d'une machine virtuelle sur le disque a échoué pour les raisons décrites dans le message d'événement.  Cet état peut se produire pour l'une de plusieurs raisons, par exemple l'incapacité d'accéder au disque.\
           </description>\
        <action>\
	   Identifiez la raison dans le message d'événement déterminer la cause de l'échec et corriger le problème.\
         </action>\
	</cause>\
     </EventLongDescription>\
     "

VmAcquiredMksTicketEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmAcquiredMksTicketEvent">\
        <description>\
           Acquisition du ticket MKS réussie pour la machine virtuelle\
        </description>\
        <cause>\
           <description>\
               Le ticket MKS utilisé pour la connexion à la console distante de la machine virtuelle  a été acquis.\
           </description>\
        </cause>\
     </EventLongDescription>\
     "

VmWwnAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnAssignedEvent">\
      <description>\
         La machine virtuelle s'est vue assigner un WWN (World Wide Name)\
      </description>\
      <cause>\
         <description>La machine virtuelle s'est vue assigner un WWN car elle  a été créée avec un disque RDM (Raw Device Mappings) ou car elle a été reconfigurée pour accéder à un disque RDM\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmWwnChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnChangedEvent">\
      <description>\
         Le WWN (World Wide Name) assigné à la machine virtuelle a été changé \
      </description>\
      <cause>\
         <description>La machine virtuelle s'est vue assigner un nouveau WWN, probablement en raison  d'un conflit provoqué par une autre machine virtuelle à laquelle  le même WWN était assigné\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmWwnConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmWwnConflictEvent">\
      <description>\
Le WWN (World Wide Name) assigné à la machine virtuelle présente un  conflit \
      </description>\
      <cause>\
         <description>Le WWN assigné à cette machine virtuelle était identique  à celui d'une machine virtuelle différente.\
       </description>\
      <action>\
	    Vérifiez les détails de l'événement pour plus informations sur le conflit et corrigez le problème.\
         </action>\</cause>\
   </EventLongDescription>\
   "

VmReloadFromPathFailedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmReloadFromPathFailedEvent">\
      <description>\
         Échec de rechargement de la machine virtuelle à partir d'un nouveau chemin de banque de données \
      </description>\
      <cause>\
         <description>Le chemin de la banque de données de destination était inaccessible ou  incorrect\
         </description>\
         <action>Utilisez un chemin de banque de données de destination correct \
         </action> \
      </cause>\
      <cause>\
         <description>La machine virtuelle est dans un état incorrect\
         </description>\
         <action>Vérifiez l'état d'alimentation de la machine virtuelle. Si la machine virtuelle est sous tension, mettez-la hors tension \
         </action>\
      </cause>\
      <cause>\
         <description>La machine virtuelle est activée pour Fault Tolerance\
         </description>\
         <action>Arrêter Fault Tolerance pour la machine  virtuelle et retentez l'opération \
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationLinuxIdentityFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationLinuxIdentityFailed">\
      <description>\
        La distribution de Linux du système d'exploitation client n'est pas prise en charge par les scripts de personnalisation.  Reportez-vous à la matrice de compatibilité VMware vSphere pour la liste des distributions Linux  prises en charge.\
      </description>\
      <cause>\
         <description>\
            La personnalisation de la distribution de Linux du système d'exploitation client cible n'est pas prise en charge.\
         </description>\
         <action>\
           Consultez VMware sur la date à partir de laquelle la distribution de Linux spécifique  sera prise en charge.  Si la distribution Linux est déjà prise en charge dans une version plus récente, envisagez une mise à niveau.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationNetworkSetupFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationNetworkSetupFailed">\
      <description>\
        Les scripts de personnalisation ont échoué à définir les paramètres dans les fichiers de configuration correspondants pour Linux ou dans  le Registre de Windows\
      </description>\
      <cause>\
         <description>\
            La spécification de personnalisation contient un nom d'hôte ou un nom de domaine incorrect\
         </description>\
         <action>\
            Examinez les fichiers journaux du système d'exploitation client pour cet événement pour plus de détails\
         </action>\
         <action>\
            Fournissez un nom d'hôte valide pour le système d'exploitation client cible. Le nom doit être conforme  aux définitions de nom d'hôte et de nom de domaine dans RFC 952, 1035, 1123, 2181.\
         </action>\
      </cause>\
      <cause>\
         <description>\
            N'a pas pu trouver de NIC avec l'adresse MAC spécifiée dans le module de personnalisation \
         </description>\
         <action>\
            Examinez les fichiers journaux du système d'exploitation client pour cet événement pour plus de détails\
         </action>\
         <action>\
            Confirmez qu'il n'y avait aucun changement dans l'adresse MAC NIC virtuelle entre la création du module de personnalisation et son déploiement. Le déploiement se produit pendant le premier démarrage de la machine virtuelle après que la personnalisation a été programmée.\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Le code de personnalisation a besoin des autorisations de lecture/écriture pour certains fichiers de configuration.  On n'a pas accordé ces autorisations au compte "racine" sur Linux ou au compte utilisé par VMware Tools Service sur le système d'exploitation client de Windows.\
         </description>\
         <action>\
            Examinez les fichiers journaux du système d'exploitation client pour cet événement pour plus de détails\
         </action>\
         <action>\
            Accorder les permissions de lecture/écriture du compte "racine" pour Linux ou du compte utilisé par VMware Tools Service sur le système d'exploitation client de Windows et les clés du registre qui doivent être modifiées par le code de personnalisation\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationSysprepFailed.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationSysprepFailed">\
      <description>\
         Les fichiers sysprep  dans le dossier correspondant au système d'exploitation client cible ne sont pas compatibles avec la version réelle du système d'exploitation client\
      </description>\
      <cause>\
         <description>\
            Les fichiers sysprep dans le dossier correspondant au système d'exploitation client cible (par exemple Windows XP) sont pour un système d'exploitation client différent (par exemple Windows 2003)\
         </description>\
         <action>\
            Sur la machine exécutant vCenter Server, placez les fichiers sysprep corrects dans le dossier  correspondant au système d'exploitation client cible\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Les fichiers sysprep dans le dossier correspondant au système d'exploitation client sont pour  un Service Pack différent, par exemple le système d'exploitation client est Windows XP SP2  mais les fichiers sysprep sont pour Windows XP SP1.\
         </description>\
         <action>\
            Sur la machine exécutant vCenter Server, placez les fichiers sysprep corrects dans le dossier  correspondant au système d'exploitation client cible\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

CustomizationUnknownFailure.longDescription = "\
   <EventLongDescription id="vim.event.CustomizationUnknownFailure">\
      <description>\
         Le composant de personnalisation pour définir les paramètres requis à l'intérieur du système d'exploitation client manque\
      </description>\
      <cause>\
         <description>\
            Sous Windows, le compte utilisateur sous lequel le code de personnalisation fonctionne n'a aucune autorisation de lecture/écriture pour les clés du registre utilisées par le code de personnalisation.  Le code de personnalisation est habituellement exécuté sous le compte "système local" mais vous pouvez changer ceci en sélectionnant un compte différent pour l'exécution de VMware Tools Service.\
         </description>\
         <action>\
            Examinez les fichiers journaux du système d'exploitation client pour cet événement pour plus de détails\
         </action>\
         <action>\
            Déterminez quel compte utilisateur est sélectiononé pour l'exécution de VMware Tools Service et confirmez que ce compte a des autorisations de lecture/écriture sur des clés du registre utilisées par le code de personnalisation\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Sous Windows, le compte utilisateur sous lequel le code de personnalisation fonctionne n'a aucune autorisation de lecture/écriture pour les fichiers et les dossiers utilisés par le code de personnalisation.  Le code de personnalisation est habituellement exécuté sous le compte "système local" mais vous pouvez changer ceci en sélectionnant  un compte différent  pour l'exécution de VMware Tools Service.\
         </description>\
         <action>\
            Examinez les fichiers journaux du système d'exploitation client pour cet événement pour plus de détails\
         </action>\
         <action>\
            Déterminez quel compte utilisateur est sélectionné pour l'exécution de VMware Tools Service et confirmez que ce compte a des autorisations de ecture/écriture sur les fichiers et les dossiers  utilisés par le code de personnalisation\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Sous Linux, un fuseau horaire incorrect ou non pris en charge est passé aux scripts de personnalisation et, en conséquence, la configuration du fuseau horaire a échoué\
         </description>\
         <action>\
            Examinez les fichiers journaux du système d'exploitation client pour cet événement pour plus de détails\
         </action>\
         <action>\
            Confirmez qu'un fuseau horaire pris en charge est passé dans la spécification de personnalisation.\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Sous Linux, le compte "racine" du système d'exploitation client n'a pas d'autorisations  de lecture/écriture pour les fichiers de configuration dont les scripts de personnalisation ont besoin pour modifier  ("/etc/hosts ")\
         </description>\
         <action>\
            Accorder les autorisations de lecture/écriture pour les fichiers de configuration du  compte "racine" du système d'exploitation client\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Pour activer la personnalisation d'invité sous Linux, si open-vm-tools est utilisé, vous devez également installer le plug-in deployPkg.\
         </description>\
         <action>\
            Suivez les instructions kb.vmware.com/kb/2075048 pour installer le plug-in deployPkg open-vm-tools.\
         </action>\
      </cause>\
      <cause>\
         <description>\
            La personnalisation du système d'exploitation invité cible n'est pas prise en charge\
         </description>\
         <action>\
            Demandez à VMware la date à partir de laquelle la distribution Linux spécifique sera prise en charge. Si la distribution Linux est déjà prise en charge dans une version plus récente, envisagez une mise à niveau.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "
#
# FT Related Events
#

VmFailedStartingSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedStartingSecondaryEvent">\
       <description>\
          vCenter Server n'a pas pu commencer le VM secondaire en raison d'une erreur\
       </description>\
       <cause>\
            <description>\
              L'hôte distant est incompatible pour la VM secondaire.  Par exemple, cette condition peut se produire quand l'hôte n'a pas accès  au réseau ou à la banque de données de la machine virtuelle.\
            </description>\
            <action>Assurez-vous que les hôtes dans le cluster sont compatibles avec FT</action>\
        </cause>\
        <cause>\
            <description>Échec de la connexion à un hôte distant. Si l'hôte a été ajouté récemment à l'inventaire  ou juste redémarré, cela pourrait prendre un certain temps pour les empreintes SSL se propagent aux hôtes.\
            </description>\
            <action>Si le problème persiste, déconnectez et rebranchez l'hôte.</action>\
        </cause>\
        <cause>\
            <description>Échec d'enregistrement du VM secondaire sur le serveur distant</description>\
            <action>Déterminez si le serveur distant a accès au magasin de données sur lequel  la machine virtuelle avec FT réside</action>\
        </cause>\
        <cause>\
            <description>Une erreur s'est produite au démarrage du VM secondaire</description>\
            <action>Déterminez la cause de l'erreur de migration. vCenter Server essayera de redémarrer  le VM secondaire s'il le peut.</action>\
        </cause>\
    </EventLongDescription>\
    "

VmFailedUpdatingSecondaryConfig.longDescription = "\
    <EventLongDescription id="vim.event.VmFailedUpdatingSecondaryConfig">\
       <description>\
          Après un basculement, le nouveau VM primaire n'est pas parvenu à mettre à jour la configuration du VM secondaire\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "

VmFaultToleranceStateChangedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceStateChangedEvent">\
       <description>\
          L'état de Fault Tolerance de la machine virtuelle a changé\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "

VmFaultToleranceTurnedOffEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceTurnedOffEvent">\
       <description>\
          Tous les VM secondaires ont été retirés et la protection de Fault Tolerance est désactivée pour cette machine virtuelle.\
       </description>\
       <cause>\
            <description> </description>\
            <action></action>\
        </cause>\
    </EventLongDescription>\
    "


VmFaultToleranceVmTerminatedEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmFaultToleranceVmTerminatedEvent">\
       <description>\
          Un VM primaire ou un VM secondaire est devenu inactif\
       </description>\
       <cause>\
            <description> Le VM secondaire est devenu inactif parce que ses opérations  ne sont plus synchronisées avec celles du VM primaire</description>\
            <action>vSphere HA essaiera de redémarrer la VM secondaire</action>\
        </cause>\
       <cause>\
            <description> Le VM secondaire est devenu inactif parce qu'un matériel ou un échec de réseau  a fait perdre au VM primaire  sa connexion entre primaire et secondaire</description>\
            <action>vSphere HA essaiera de redémarrer la VM secondaire</action>\
        </cause>\
       <cause>\
            <description> La VM avec Fault Tolerance est devenue inactive en raison d'une panne partielle de matériel sur l'hôte physique</description>\
            <action>vSphere HA essaiera de redémarrer la VM secondaire</action>\
        </cause>\
       <cause>\
            <description> Un utilisateur a arrêté Fault Tolerance VM</description>\
            <action>La VM avec Fault Tolerance restante devient la VM principale. vSphere HA  essaiera de redémarrer la VM secondaire.</action>\
        </cause>\
    </EventLongDescription>\
    "


VmMaxFTRestartCountReached.longDescription = "\
    <EventLongDescription id="vim.event.VmMaxFTRestartCountReached">\
       <description>\
          Le système a atteint la limite maximum de redémarrages dans sa tentative de redémarrer un VM secondaire\
       </description>\
       <cause>\
            <description>Le système a dépassé le nombre de tentatives permises de redémarrages pour le VM secondaire  quand il a essayé de rétablir Fault Tolerance</description>\
            <action>Examinez les causes d'échec de redémarrage et résolvez-les. Désactivez puis réactivez la protection de Fault Tolerance.</action>\
        </cause>\
    </EventLongDescription>\
    "

VmNoCompatibleHostForSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmNoCompatibleHostForSecondaryEvent">\
       <description>\
          Aucun hôte compatible n'a été trouvé en essayant de placer un VM secondaire\
       </description>\
       <cause>\
            <description>Il n'y avait aucun hôte compatible disponible pour placer un VM secondaire</description>\
            <action>Résolvez les incompatibilités et relancez l'opération</action>\
        </cause>\
    </EventLongDescription>\
    "

VmSecondaryDisabledBySystemEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmSecondaryDisabledBySystemEvent">\
       <description>\
          vCenter Server a désactivé un VM secondaire parce qu'il ne pouvait pas mettre le VM secondaire sous tension\
       </description>\
       <cause>\
            <description>vCenter Server n'a pas réussi à mettre le VM secondaire sous tension </description>\
            <action>Examinez la raison dans le message de l'événement pour plus de détails, résolvez l'erreur et  réactivez la protection de Fault Tolerance pour mettre le VM secondaire sous tension.</action>\
        </cause>\
    </EventLongDescription>\
    "


VmTimedoutStartingSecondaryEvent.longDescription = "\
    <EventLongDescription id="vim.event.VmTimedoutStartingSecondaryEvent">\
       <description> Une tentative de démarrage d'un VM secondaire est arrivée au bout de son délai.\
       </description>\
       <cause>\
            <description>Un utilisateur a essayé d'allumer ou d'activer Fault Tolerance, déclenchant le démarrage  du VM secondaire. L'opération de démarrage est arrivée au bout de son délai et, en conséquence, vCenter Server désactive Fault Tolerance. </description>\
            <action>Résolvez tout problème et réactivez la protection de Fault Tolerance</action>\
        </cause>\
       <cause>\
            <description>Le VM secondaire a été démarré en réponse à un échec, mais la tentative est arrivée  au bout de son délai</description>\
            <action> vSphere HA essayera de mettre la VM secondaire sous tension</action>\
        </cause>\
    </EventLongDescription>\
    "

CanceledHostOperationEvent.longDescription = "\
	<EventLongDescription id="vim.event.CanceledHostOperationEvent">\
		<description>\
			Une opération exécutée sur l'hôte a été annulée\
		</description>\
		<cause>\
			<description>\
		Un événement précédent dans la séquence d'événements fournira plus d'informations au sujet de la cause de cette annulation\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

ChangeOwnerOfFileFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.ChangeOwnerOfFileFailedEvent">\
		<description>\
			Le changement de propriétaire d'un fichier ou d'un dossier a échoué\
		</description>\
	</EventLongDescription>\
    "

HostAddFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostAddFailedEvent">\
		<description>\
			L'ajout d'un hôte a échoué\
		</description>\
	</EventLongDescription>\
    "

HostAdminDisableEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostAdminDisableEvent">\
		<description>\
			Des autorisations d'hôte ont été modifiées afin que seul le compte utilisé pour des opérations de vCenter Server ait des autorisations d'administrateur\
		</description>\
		<cause>\
			<description>\
				Cette condition se produit quand vCenter Server supprime tout autre accès administrateur vers l'hôte étant donné que l'hôte a été placé en mode Verrouillage. L'hôte peut être géré par vCenter Server uniquement et seul vCenter Server peut réactiver l'accès administrateur pour les autres comptes.\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

HostAdminEnableEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostAdminEnableEvent">\
		<description>\
			vCenter Server a restauré des permissions d'administrateur pour les comptes utilisateurs d'hôte dont les autorisations ont été désactivées par mode verrouillage\
		</description>\
		<cause>\
			<description>\
				Cette condition se produit quand vCenter Server restaure l'accès d'administrateur aux comptes utilisateurs d'hôte qui ont détruit leurs permissions d'administrateur quand l'hôte a été placé en mode verrouillage\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedAccountFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedAccountFailedEvent">\
		<description>\
			N'a pas pu le connecter à l'hôte parce que l'installation d'un compte de gestion a échoué\
		</description>\
		<cause>\
			<description>\
			Le compte utilisé par vCenter Server pour gérer l'hôte n'a pas pu être configuré\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedAlreadyManagedEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedAlreadyManagedEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte parce qu'il est déjà géré par une instance différente de vCenter Server.\
		</description>\
		<cause>\
			<description>\
				L'hôte est déjà géré par une instance différente de vCenter Server\
			</description>\
			<action>\
				Retirez l'hôte de l'inventaire pour l'autre instance de vCenter Server\
			</action>\
			<action>\
				Oblige l'ajout de l'hôte à l'instance actuelle de vCenter Server\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedBadCcagentEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedBadCcagentEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte parce que l'agent hôte n'a pas répondu\
		</description>\
		<cause>\
			<description>\
				Aucune réponse n'a été reçue de l'agent hôte\
			</description>\
			<action>\
				Redémarrez l'agent hôte sur l'hôte d'ESX/ESXi\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedBadUsernameEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedBadUsernameEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte en raison d'une combinaison incorrecte de nom d'utilisateur et de mot de passe\
		</description>\
		<cause>\
			<description>\
				Combinaison incorrecte de nom d'utilisateur et de mot de passe\
			</description>\
			<action>\
				Utilisez le nom d'utilisateur et le mot de passe corrects\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedBadVersionEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedBadVersionEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte en raison d'une version incompatible de vSphere Client\
		</description>\
		<cause>\
			<description>\
			La version de vSphere Client est incompatible avec l'hôte d'ESX/ESXi, aussi la tentative de connexion a échoué\
			</description>\
			<action>\
			Téléchargez et employez une version compatible de vSphere Client pour vous connecter à l'hôte\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedCcagentUpgradeEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedCcagentUpgradeEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte parce qu'une mise à niveau ou une installation d'agent hôte est en cours\
		</description>\
		<cause>\
			<description>\
				L'agent hôte est mis à niveau ou installé sur l'hôte\
			</description>\
			<action>\
				Attente de mise à niveau ou installation à compléter\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte en raison d'un état non spécifié\
		</description>\
		<cause>\
			<description>\
				Cause inconnue d'échec\
			</description>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedNetworkErrorEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedNetworkErrorEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte en raison d'une erreur réseau\
		</description>\
		<cause>\
			<description>\
				Une erreur réseau est survenue en connectant l'hôte\
			</description>\
			<action>\
				Vérifiez que la gestion de réseau d'hôte est configurée correctement\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedNoAccessEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedNoAccessEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte en raison des privilèges insuffisants de compte\
		</description>\
		<cause>\
			<description>\
				Le compte utilisé pour se connecter à l'hôte n'a pas de de privilèges d'accès\
			</description>\
			<action>\
				Utilisez un compte qui a des privilèges suffisants pour se connecter à l'hôte\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedNoConnectionEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedNoConnectionEvent">\
		<description>\
			N'a pas pu seconnecter à l'hôte parce que l'hôte n'est pas dans le réseau\
		</description>\
		<cause>\
			<description>\
				L'hôte au lequel vous essayez de vous connecter n'est pas présent dans le réseau\
			</description>\
			<action>\
				Vérifiez que la gestion de réseau de l'hôte est configurée correctement et que l'hôte est connecté au même réseau que vCenter Server\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedNoLicenseEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedNoLicenseEvent">\
		<description>\
			N'a pas pu se connecter à l'hôte en raison d'un problème de licence\
		</description>\
		<cause>\
			<description>\
				Il n'y a pas assez de licences pour ajouter l'hôte à l'inventaire de vCenter Server. Cet événement est accompagné d'une défaillance qui spécifie les licences manquantes exigées pour ajouter l'hôte avec succès.\
			</description>\
			<action>\
				Ajoutez les licences nécessaires à vCenter Server et réessayez d'ajouter l'hôte\
			</action>\
		</cause>\
	</EventLongDescription>\
    "

HostCnxFailedNotFoundEvent.longDescription = "\
	<EventLongDescription id="vim.event.HostCnxFailedNotFoundEvent">\
