HostInventoryFullEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostInventoryFullEvent">\
       <description>\
          現在、vCenter Server Foundation ライセンス キーを使用してインベントリに追加できるのは 3 台のホストのみです。それを超える数のホストを追加するとエラーが発生し、このイベントがログに記録されます。\
       </description>\
       <cause>\
            <description>vCenter Server に割り当てられているライセンス キーで許可される数よりも多くのホストを 追加しようとしています。</description>\
            <action>より多くのホストを許可するライセンス キーか、ホスト数に制限のないライセンス キーを vCenter Server に 割り当ててください。</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseNonComplianceEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseNonComplianceEvent">\
       <description>\
          vCenter Server では、利用可能なライセンス数と利用中のエンティティ数が厳密に一致していませんが、上限を超えるライセンス数が使用されていないかどうか定期的に確認しています。 vCenter Server は上限を超えるライセンス数の使用を検出すると、このイベントをログに記録し、アラームを起動します。\
       </description>\
       <cause>\
            <description>上限を超えるライセンス数の使用</description>\
            <action>vSphere Client を使用してライセンス レポートを確認し、ライセンス キーを使用するエンティティの数を減らすか、キャパシティを増やすため新しいライセンス キーを追加してください</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseServerUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseServerUnavailableEvent">\
       <description>\
          vCenter Server はライセンス サーバの状態を追跡し、ライセンス サーバが応答を停止した場合はこのイベントをログに記録します。\
       </description>\
       <cause>\
            <description>ライセンス サーバは応答しておらず、vCenter Server で利用できません。</description>\
            <action>ライセンス サーバが実行されていることを確認してください。 サーバが実行されている場合は、vCenter Server とライセンス サーバの間の接続を確認してください。</action>\
       </cause>\
    </EventLongDescription>\
    "

NoLicenseEvent.longDescription = "\
    <EventLongDescription id="vim.event.NoLicenseEvent">\
       <description>\
          vCenter Server は、不明な原因により、ライセンス サーバから機能を利用するためのライセンスが取得できない場合に、このイベントをログに記録します。 \
       </description>\
       <cause>\
            <description>不明な原因により機能を利用するためのライセンスを取得できません。</description>\
            <action>ライセンス サーバに該当の機能を使用するためのライセンスがあることを確認してください。</action>\
       </cause>\
    </EventLongDescription>\
    "


InvalidEditionEvent.longDescription = "\
    <EventLongDescription id="vim.event.InvalidEditionEvent">\
       <description>\
          vCenter Server はライセンス サーバから未定義の機能を取得しようとしました。\
       </description>\
       <cause>\
            <description>vMotion、DRS、vSphere HA など、ライセンスが必要な機能の操作を行うと、該当する機能がライセンス サーバで定義されていない場合にこのイベントが発生することがあります。</description>\
            <action>該当する機能のライセンスがライセンス サーバに存在していることを確認してください。</action>\
       </cause>\
    </EventLongDescription>\
    "

HostLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostLicenseExpiredEvent">\
       <description>\
          vCenter Server は、ライセンス サーバ上のホスト ライセンスの有効期限を追跡し、期限切れが近いホスト ライセンスがある場合はこのイベントを使用して通知を行います。\
       </description>\
       <cause>\
            <description>ライセンス サーバ上のホスト ライセンスの有効期間がもうすぐ期限切れとなります。</description>\
            <action>ライセンス サーバを更新して新しいバージョンのホスト ライセンスを取得してください。</action>\
       </cause>\
    </EventLongDescription>\
    "

VMotionLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.VMotionLicenseExpiredEvent">\
       <description>\
          vCenter Server は、ライセンス サーバ上の vMotion ライセンスの有効期限を追跡し、期限切れが近い vMotion ライセンスがある場合はこのイベントを使用して通知を行います。\
       </description>\
       <cause>\
            <description>ライセンス サーバ上の vMotion ライセンスの有効期間がもうすぐ期限切れとなります。</description>\
            <action>ライセンス サーバを更新して新しいバージョンの vMotion ライセンスを取得してください。</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseRestrictedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseRestrictedEvent">\
       <description>\
          vCenter Server は、ライセンス ファイルの制限によりライセンス サーバからライセンスを チェックアウトできない場合に、このイベントをログに記録します。\
       </description>\
       <cause>\
            <description>ライセンス サーバ上のライセンス ファイルに制限があるためチェックアウトできません。</description>\
            <action>ライセンス ファイルを確認し、解除できる制限は解除してください。</action>\
       </cause>\
    </EventLongDescription>\
    "

VmFailedToPowerOffEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOffEvent">\
      <description>仮想マシンのパワーオフに失敗しました。</description>\
      <cause>\
         <description>\
            仮想マシンで同時に操作が行われている可能性があります。\
         </description>\
         <action>すべての操作を終了し、パワーオフ操作をやり直してください。</action>\
      </cause>\
      <cause>\
         <description>仮想マシンが無効な状態です。 データストアにアクセスできないなどのさまざまな理由により、 仮想マシンは無効な状態になることがあります。 </description>\
         <action>\
            仮想マシンが無効な状態になった理由を特定し、問題を解決して、操作をやり直してください。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToPowerOnEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOnEvent">\
      <description>\
         仮想マシンのパワーオフに失敗しました。\
      </description>\
      <cause>\
         <description>\
            仮想マシンがすでにパワーオン状態になっている、仮想マシン上で同時に操作が実行されているなどの理由により、仮想マシンのパワーオンに失敗することがあります。 </description>\
         <action>\
	  イベント メッセージで理由を確認し、パワーオンの失敗の原因を見つけて、問題を修正してください。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToRebootGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToRebootGuestEvent">\
      <description>\
         仮想マシン上のゲスト OS の再起動に失敗しました。\
      </description>\
      <cause>\
         <description>\
            仮想マシンがパワーオン状態になっていない、仮想マシン上で同時に操作が実行されているなどの理由により、ゲスト OS の再起動に失敗することがあります。\
         </description>\
	 <action>\
	  イベント メッセージで理由を確認し、再起動の失敗の原因を見つけて、問題を修正してください。\
         </action>\
       </cause>\
   </EventLongDescription>\
   "

VmFailedToResetEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToResetEvent">\
      <description>仮想マシンのリセットに失敗しました。</description>\
      <cause>\
         <description>\
		仮想マシンはユーザーへの質問またはプロンプトへの応答を待っている可能性があります。</description>\
         <action>\
            vSphere Client で仮想マシンの「サマリ」タブに移動し、ユーザーへの質問またはプロンプトに応答してください。\
         </action>\
      </cause>\
      <cause>\
         <description>この操作を実行可能なライセンスが存在しない可能性があります。</description>\
         <action>\
            必要なライセンスを取得して、リセット操作をやり直してください。\
         </action>\
      </cause>\
      <cause>\
         <description>\
            仮想マシン上で同時に操作が行われている可能性があります。\
         </description>\
         <action>すべての操作を終了し、リセット操作をやり直してください。</action>\
      </cause>\
      <cause>\
         <description>\
            仮想マシンを実行しているホストがメンテナンス モードになっています。\
         </description>\
         <action>\
           ホストのメンテナンス モードが終了するまで待機し、操作をやり直してください。\
         </action>\
      </cause>\
      <cause>\
         <description>仮想マシンが無効な状態です。 データストアにアクセスできないなどのさまざまな理由により、仮想マシンは無効な状態になることがあります。</description>\
         <action>\
            仮想マシンが無効な状態になった理由を特定し、問題を解決して、操作をやり直してください。\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

