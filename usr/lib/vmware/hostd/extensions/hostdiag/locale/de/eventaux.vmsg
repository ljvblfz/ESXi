com.vmware.vc.HA.CreateConfigVvolFailedEvent.longDescription = "\
   <EventLongDescription id="com.vmware.vc.HA.CreateConfigVvolFailedEvent">\
      <description>\
        vSphere HA konnte kein Konfigurations-vVol auf dem Datenspeicher erstellen \
     </description>\
      <cause>\
           <description>VP, Host, Netzwerk oder ein Mangel an Ressourcen haben möglicherweise verhindert, dass vSphere HA ein Konfigurations-vVol erstellen konnte.</description>\
           <action>Suchen Sie nach Fehlern in der Umgebung und aktivieren Sie dann vSphere HA erneut</action>\
      </cause>\
   </EventLongDescription>\

   "

com.vmware.vc.HA.CreateConfigVvolSucceededEvent.longDescription = "\
   <EventLongDescription id="com.vmware.vc.HA.CreateConfigVvolSucceededEvent">\
      <description>\
        vSphere HA hat das Konfigurations-vVol erfolgreich auf dem Datenspeicher erstellt. Falls zuvor das Erstellen eines Konfigurations-vVol bei der Konfiguration des Datenspeichers fehlgeschlagen ist, ist dieses Problem jetzt behoben \
     </description>\
      <cause>\
           <description> Beim Erstellen des Konfigurations-vVol auf dem Datenspeicher ist kein Fehler aufgetreten</description>\
      </cause>\
   </EventLongDescription>\

   "

vim.event.LicenseDowngradedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseDowngradedEvent">\
       <description>\
          Die installierte Lizenz reduziert den Satz der verfügbaren Funktionen. Mit der neuen Lizenz sind einige der Funktionen, die früher zur Verfügung standen, nicht mehr zugänglich.\
       </description>\
       <cause>\
            <description>Die Lizenz wurde ersetzt.</description>\
            <action>Stellen Sie die vorher installierte Lizenz wieder her, sofern sie noch nicht abgelaufen ist.</action>\
            <action>Wenden Sie sich an VMware, um eine neue Lizenz mit den benötigten Funktionen zu erwerben.</action>\
        </cause>\
    </EventLongDescription>\

    "

vim.event.SystemSwapInaccessible.longDescription = "\
    <EventLongDescription id="vim.event.SystemSwapInaccessible">\
       <description>\
          Die Systemauslagerung wurde auf diesem System konfiguriert und verwendet, aber der dazu verwendete Datenspeicher ist nicht mehr verfügbar.\
          Dies führt möglicherweise zu geringerer Leistung und möglichen Abstürzen einiger Benutzer-Worlds, deren Arbeitsspeicher in die Systemauslagerungsdatei ausgelagert wurde.\
          Ein weiteres beobachtetes Problem besteht darin, dass der Datenspeicher aufgrund der Referenzen auf ihn nicht ordnungsgemäß aus der Bestandsliste des Hosts entfernt werden kann.\
       </description>\
       <cause>\
            <description>Die zugrunde liegende LUN hat den Status „Keine Pfade verfügbar“, wurde physisch entfernt oder es sind Fehler in der Kommunikation mit diesem Gerät aufgetreten.</description>\
            <action>Konfigurieren Sie die Systemauslagerung neu oder ändern Sie den Speicherort der Auslagerungsdatei des Hostsystems</action>\
        </cause>\
    </EventLongDescription>\

    "

