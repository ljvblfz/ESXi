DatastoreDiscoveredEvent.longDescription = "\
  <EventLongDescription id="vim.event.DatastoreDiscoveredEvent">\
     <description>\
        在主机上发现数据存储\
     </description>\
     <cause>\
        <description>\
           向数据中心添加了可以访问此数据存储的主机\
        </description>\
     </cause>\
     <cause>\
        <description>\
           未向数据中心中的主机屏蔽备份此数据存储的存储\
        </description>\
     </cause>\
     <cause>\
        <description>\
