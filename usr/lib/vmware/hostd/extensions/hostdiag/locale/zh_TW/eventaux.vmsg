com.vmware.vc.HA.CreateConfigVvolFailedEvent.longDescription = "\
   <EventLongDescription id="com.vmware.vc.HA.CreateConfigVvolFailedEvent">\
      <description>\
        vSphere HA 無法在資料存放區上建立組態 vvol \
     </description>\
      <cause>\
           <description>可能因 VP、主機、網路或資源不足而導致 vSphere HA 無法建立組態 vvol</description>\
           <action>查看環境中是否存在錯誤，然後重新啟用 vSphere HA</action>\
      </cause>\
   </EventLongDescription>\

   "

com.vmware.vc.HA.CreateConfigVvolSucceededEvent.longDescription = "\
   <EventLongDescription id="com.vmware.vc.HA.CreateConfigVvolSucceededEvent">\
      <description>\
        vSphere HA 已在資料存放區上成功建立組態 vvol。如果曾經發生無法在資料存放區上設定組態 vvol 的問題，將清除該問題 \
     </description>\
      <cause>\
           <description> 在資料存放區上建立組態 vvol 時未發生錯誤</description>\
      </cause>\
   </EventLongDescription>\

   "

vim.event.LicenseDowngradedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseDowngradedEvent">\
       <description>\
          安裝的授權會減少可用功能集。使用新授權將無法存取先前可用的部分功能。\
       </description>\
       <cause>\
            <description>該授權已更換。</description>\
            <action>若尚未到期，還原為先前安裝的授權。</action>\
            <action>請連絡 VMware，以取得具有所需功能的新授權。</action>\
        </cause>\
    </EventLongDescription>\

    "

vim.event.SystemSwapInaccessible.longDescription = "\
    <EventLongDescription id="vim.event.SystemSwapInaccessible">\
       <description>\
          已在此系統上設定並使用系統交換，但用於此系統交換的資料存放區不再可用。\
          可能會使已將記憶體交換到系統分頁檔的某些使用者環境遇到效能降低和當機情況。\
          發現的另一個問題是，由於資料存放區存在保留的參考項目，因此無法將其從主機的詳細目錄中適當移除。\
       </description>\
       <cause>\
            <description>基礎 LUN 處於「所有路徑失效」狀態、已實際移除，或者與此裝置的通訊發生錯誤。</description>\
            <action>重新設定系統交換組態或變更主機系統的分頁檔位置</action>\
        </cause>\
    </EventLongDescription>\

    "

