HostInventoryFullEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostInventoryFullEvent">\
       <description>\
          Der Lizenzschlüssel für vCenter Server Foundation ermöglicht gegenwärtig, dass nur drei Hosts zur Bestandsliste hinzugefügt werden können. Das Hinzufügen zusätzlicher Hosts führt zu Fehlern sowie zur Protokollierung dieses Ereignisses. \
       </description>\
       <cause>\
            <description>Es wurde versucht, mehr Hosts als die maximale Anzahl an Hosts hinzuzufügen, die durch den vCenter Server zugewiesenen Lizenzschlüssel festgelegt ist</description>\
            <action>Weisen Sie vCenter Server einen Lizenzschlüssel zu, der mehr Hosts zulässt oder kein Hostlimit hat</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseNonComplianceEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseNonComplianceEvent">\
       <description>\
          vCenter Server sorgt nicht für die strikte Einhaltung der durch die Lizenz festgelegten Grenzwerte. Es wird regelmäßig überprüft, ob die Lizenzen überbeansprucht sind. Wenn vCenter Server eine Überbeanspruchung feststellt, wird dieses Ereignis protokolliert und ein Alarm ausgelöst.\
       </description>\
       <cause>\
            <description>Überbeanspruchung der Lizenzen</description>\
            <action>Überprüfen Sie die Lizenzberichte mit dem vSphere Client und reduzieren Sie die Anzahl der Entitäten, die den Lizenzschlüssel verwenden, oder fügen Sie einen neuen Lizenzschlüssel mit größerer Kapazität hinzu</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseServerUnavailableEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseServerUnavailableEvent">\
       <description>\
          vCenter Server verfolgt den Zustand des Lizenzservers und protokolliert dieses Ereignis, wenn der Lizenzserver nicht mehr reagiert. \
       </description>\
       <cause>\
            <description>Der Lizenzserver reagiert nicht und steht vCenter Server nicht zur Verfügung</description>\
            <action>Stellen Sie sicher, dass der Lizenzserver ausgeführt wird. Ist dies der Fall, überprüfen Sie die Verbindung zwischen vCenter Server und dem Lizenzserver.</action>\
       </cause>\
    </EventLongDescription>\
    "

NoLicenseEvent.longDescription = "\
    <EventLongDescription id="vim.event.NoLicenseEvent">\
       <description>\
          vCenter Server protokolliert dieses Ereignis, falls er aus unbekanntem Grund nicht in der Lage ist, eine Funktionslizenz des Lizenzservers abzurufen. \
       </description>\
       <cause>\
            <description>Das Abrufen einer Funktionslizenz schlägt aus unbekanntem Grund fehl</description>\
            <action>Stellen Sie sicher, dass der Lizenzserver über die Lizenz für die Funktion verfügt</action>\
       </cause>\
    </EventLongDescription>\
    "


InvalidEditionEvent.longDescription = "\
    <EventLongDescription id="vim.event.InvalidEditionEvent">\
       <description>\
          vCenter Server hat versucht, eine nicht definierte Funktion vom Lizenzserver abzurufen \
       </description>\
       <cause>\
            <description>Ein Vorgang, der eine Funktionslizenz wie z. B. vMotion, DRS oder vSphere HA benötigt, führt möglicherweise zu diesem Ereignis, falls diese Funktion nicht auf dem Lizenzserver definiert ist</description>\
            <action>Stellen Sie sicher, dass die fragliche Funktion auf dem Lizenzserver verfügbar ist</action>\
       </cause>\
    </EventLongDescription>\
    "

HostLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.HostLicenseExpiredEvent">\
       <description>\
          vCenter Server verfolgt die Ablauftermine der Hostlizenzen auf dem Lizenzserver und benachrichtigt Sie anhand dieses Ereignisses über Hostlizenzen, die in Kürze ablaufen\
       </description>\
       <cause>\
            <description>Hostlizenzen auf dem Lizenzserver laufen in Kürze ab</description>\
            <action>Aktualisieren Sie den Lizenzserver, um eine neue Version der Hostlizenz abzurufen</action>\
       </cause>\
    </EventLongDescription>\
    "

VMotionLicenseExpiredEvent.longDescription = "\
    <EventLongDescription id="vim.event.VMotionLicenseExpiredEvent">\
       <description>\
          vCenter Server verfolgt die Ablauftermine der vMotion-Lizenzen auf dem Lizenzserver und benachrichtigt Sie anhand dieses Ereignisses über vMotion-Lizenzen, die in Kürze ablaufen\
       </description>\
       <cause>\
            <description>vMotion-Lizenzen auf dem Lizenzserver laufen in Kürze ab</description>\
            <action>Aktualisieren Sie den Lizenzserver, um eine neue Version der vMotion-Lizenz abzurufen</action>\
       </cause>\
    </EventLongDescription>\
    "

LicenseRestrictedEvent.longDescription = "\
    <EventLongDescription id="vim.event.LicenseRestrictedEvent">\
       <description>\
          vCenter Server protokolliert dieses Ereignis, wenn er nicht in der Lage ist, aufgrund von Beschränkungen in der Lizenzdatei eine Lizenz vom Lizenzserver auszuchecken \
       </description>\
       <cause>\
            <description>Die Lizenzdatei auf dem Lizenzserver unterliegt Beschränkungen, die das Auschecken verhindern</description>\
            <action>Überprüfen Sie die Lizenzdatei und entfernen Sie alle Beschränkungen, sofern möglich</action>\
       </cause>\
    </EventLongDescription>\
    "

VmFailedToPowerOffEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOffEvent">\
      <description>Das Ausschalten der virtuellen Maschine ist fehlgeschlagen</description>\
      <cause>\
         <description>\
            Die virtuelle Maschine führt möglicherweise gleichzeitige Vorgänge durch\
         </description>\
         <action>Schließen Sie die gleichzeitigen Vorgänge ab und führen Sie den Ausschaltvorgang erneut aus</action>\
      </cause>\
      <cause>\
         <description>Die virtuelle Maschine ist in einem ungültigen Zustand. Virtuelle Maschinen können aus vielen Gründen einen ungültigen Zustand haben, beispielsweise dann, wenn nicht auf den Datenspeicher zugegriffen werden kann. </description>\
         <action>\
            Identifizieren Sie den Grund, warum die virtuelle Maschine in einen ungültigen Zustand eingetreten ist, beheben Sie das Problem und führen Sie den Vorgang erneut aus.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToPowerOnEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToPowerOnEvent">\
      <description>\
         Das Einschalten der virtuellen Maschine ist fehlgeschlagen\
      </description>\
      <cause>\
         <description>\
            Einschaltversuche für virtuelle Maschinen können fehlschlagen, weil sich die virtuelle Maschine bereits in einem eingeschalteten Zustand befindet, gleichzeitige Vorgänge auf der virtuellen Maschine ausgeführt werden, usw. </description>\
         <action>\
	  Überprüfen Sie den Grund in der Ereignismeldung, um die Ursache für das Fehlschlagen des Einschaltens herauszufinden, und beheben Sie das Problem.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmFailedToRebootGuestEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToRebootGuestEvent">\
      <description>\
         Der Neustart des Gastbetriebssystems auf der virtuellen Maschine ist fehlgeschlagen.\
      </description>\
      <cause>\
         <description>\
            Neustarts des Gastbetriebssystems können fehlschlagen, weil sich die virtuelle Maschine nicht in einem eingeschalteten Zustand befindet, gleichzeitige Vorgänge auf der virtuellen Maschine ausgeführt werden, usw.\
         </description>\
	 <action>\
	  Überprüfen Sie den Grund in der Ereignismeldung, um die Ursache für das Fehlschlagen des Neustarts herauszufinden, und beheben Sie das Problem.\
         </action>\
       </cause>\
   </EventLongDescription>\
   "

VmFailedToResetEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmFailedToResetEvent">\
      <description>Die virtuelle Maschine konnte nicht zurückgesetzt werden</description>\
      <cause>\
         <description>\
		Die virtuelle Maschine wartet möglicherweise auf eine Antwort auf eine Frage oder auf eine Reaktion auf eine Aufforderung</description>\
         <action>\
            Wechseln Sie im vSphere Client zur Registerkarte „Übersicht“ der virtuellen Maschine und antworten Sie auf die Frage oder Aufforderung\
         </action>\
      </cause>\
      <cause>\
         <description>Für diesen Vorgang stehen möglicherweise nicht genügend Lizenzen zur Verfügung.</description>\
         <action>\
            Erwerben Sie die erforderlichen Lizenzen und führen Sie den Rücksetzungsvorgang erneut aus\
         </action>\
      </cause>\
      <cause>\
         <description>\
            Auf der virtuellen Maschine werden möglicherweise gleichzeitige Vorgänge ausgeführt\
         </description>\
         <action>Schließen Sie die gleichzeitigen Vorgänge ab und führen Sie den Rücksetzungsvorgang erneut aus</action>\
      </cause>\
      <cause>\
         <description>\
            Der Host, auf dem die virtuelle Maschine ausgeführt wird, wechselt in den Wartungsmodus\
         </description>\
         <action>\
            Warten Sie, bis der Host den Wartungsmodus verlässt, und führen Sie den Vorgang erneut aus\
         </action>\
      </cause>\
      <cause>\
         <description>Die virtuelle Maschine ist in einem ungültigen Zustand. Virtuelle Maschinen können aus vielen Gründen einen ungültigen Zustand haben, beispielsweise dann, wenn nicht auf den Datenspeicher zugegriffen werden kann.</description>\
         <action>\
            Identifizieren Sie den Grund, warum die virtuelle Maschine in einen ungültigen Zustand eingetreten ist, beheben Sie das Problem und führen Sie den Vorgang erneut aus.\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmUpgradingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUpgradingEvent">\
      <description>Die virtuelle Hardware auf dieser virtuellen Maschine wird aktualisiert</description>\
      <cause>\
         <description>Eine vom Benutzer initiierte Aktion hat ein Upgrade der Hardware der virtuellen Maschine ausgelöst</description>\
      </cause>\
      <cause>\
         <description>Eine geplante Aufgabe hat ein Upgrade der Hardware der virtuellen Maschine ausgelöst</description>\
      </cause>\
   </EventLongDescription>\
   "

VmUuidAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUuidAssignedEvent">\
      <description>Der virtuellen Maschine wurde eine neue BIOS UUID zugewiesen</description>\
      <cause>\
         <description>Der Benutzer, der die virtuelle Maschine erstellt hat, hat zur Erstellungszeit keine BIOS UUID angegeben. vCenter Server hat eine neue UUID generiert und sie der virtuellen Maschine zugewiesen.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidAssignedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidAssignedEvent">\
      <description>Der virtuellen Maschine wurde eine neue vCenter Server-spezifische Instanz-UUID zugewiesen\
      </description>\
      <cause>\
         <description> \
             Der Benutzer, der die virtuelle Maschine erstellt hat, hat zur Erstellungszeit keine vCenter Server-spezifische Instanz-UUID angegeben. vCenter Server hat eine neue UUID generiert und sie der virtuellen Maschine zugewiesen.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmUuidChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUuidChangedEvent">\
      <description>Die BIOS UUID der virtuellen Maschine wurde geändert</description>\
      <cause>\
         <description>\
            Ein Benutzer hat die BIOS UUID der virtuellen Maschine direkt auf dem Host geändert\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidChangedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidChangedEvent">\
      <description> \
         Die vCenter Server-spezifische Instanz-UUID der virtuellen Maschine wurde geändert\
      </description> \
      <cause>\
         <description>\
            Eine  Benutzeraktion führte zu einer Änderung der vCenter Server-spezifischen Instanz-UUID der virtuellen Maschine\
         </description>\
      </cause>\
      <cause>\
         <description>\
            vCenter Server hat die Instanz-UUID der virtuellen Maschine geändert, weil er einen Konflikt erkannt hat\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmInstanceUuidConflictEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmInstanceUuidChangedEvent">\
      <description> \
         Die vCenter Server-spezifische Instanz-UUID der virtuellen Maschine steht in Konflikt mit der einer anderen virtuellen Maschine. \
      </description>\
      <cause>\
         <description>\
             Konflikte bei der Instanz-UUID der virtuellen Maschine können auftreten, wenn Sie Dateien der virtuellen Maschine ohne Verwendung von vCenter Server manuell kopieren. \
         </description>\
      </cause>\
   </EventLongDescription>\
   "

VmRelocateFailedEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmRelocateFailedEvent">\
      <description>\
         Verlagerung der virtuellen Maschine auf einen anderen Host oder Datenspeicher fehlgeschlagen\
      </description>\
      <cause>\
         <description>\
            Die Verlagerung einer virtuellen Maschine kann aus verschiedenen Gründen fehlschlagen, einschließlich Netzwerkausfällen, nicht ausreichendem Festplattenspeicher, usw.\
         </description>\
         <action>\
            Beachten Sie die mit diesem Ereignis verbundene Aufgabe, analysieren Sie die Ursache des Fehlschlags und nehmen Sie die entsprechenden Korrekturen vor\
         </action>\
      </cause>\
   </EventLongDescription>\
   "

VmUnsupportedStartingEvent.longDescription = "\
   <EventLongDescription id="vim.event.VmUnsupportedStartingEvent">\
      <description>\
         Es wurde versucht, eine virtuelle Maschine einzuschalten, die über ein nicht unterstütztes Gastbetriebssystem verfügt\
      </description>\
      <cause>\
         <description>\
            Eine Benutzeraktion hat zu einem Einschaltvorgang einer virtuellen Maschine geführt, aber die virtuelle Maschine verfügt über ein nicht unterstütztes Gastbetriebssystem.\
         </description>\
      </cause>\
   </EventLongDescription>\
   "

